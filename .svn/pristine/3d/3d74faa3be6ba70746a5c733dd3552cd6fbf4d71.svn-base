@charset "utf-8";
h1{font-weight: bold; border-bottom:10px solid #3d3d3d; color: #3d3d3d;line-height:1.25;}
:focus{outline:0;}
em{ font-style:normal;}
a:hover{ text-decoration:none;}
section {margin-bottom: 4em;}
.fl{float: left;}
.fr{ float:right;}
.clearfix:after { content: "."; display: block; clear: both; height: 0; visibility: hidden; }
.clearfix { display: block; min-height: 1%; }
*html .clearfix { height: 1%; }
.dis-bolck{ display:block; line-height:2;}
/*====top====*/
.fixed.fixed-top-bar {z-index: 5; }
.fixed-top {left: 0; position: fixed; top: 0; width: 100%; z-index: 99;}
.navbar {background-color: #333333; height: 75px; left: 0;  line-height: 75px; margin: 0; padding: 0;  position: fixed; top: 0;width: 100%;}
.navbar-brand{background:url(../images/logo.png) no-repeat scroll left center; margin-right:20px; width: 198px; float:left; height: 75px; line-height: 75px;margin-left: 25px; overflow: hidden;}
.left-side{float:left;}
.navbar-section > li > span{ border-bottom:1px solid #777777; color: #ffffff; border-radius: 5px; display: inline-block; height: 40px; line-height: 40px; margin-top: 17.5px; padding: 0 15px 5px; position: relative;font-size: 2rem;}
.navbar-search-input{background-color: transparent; border: 0 solid transparent; border-radius: 0; color: #ffffff; display: inline-block; font-size: 17px; height: 35px;  margin: 0;  padding: 0 0 0 10px;  transition: none 0s ease 0s ;}
.right-side{float: right; position: relative;}
.navbar-section > li{float:left; font-size: 1.6rem;}
.navbar-section a{color:#ffffff;}
.navbar-section > li:nth-child(2){background-color: #3d3e3f; border-color: #333333 -moz-use-text-color;  border-style: solid none;  border-width: 15px medium; height: 70px; margin: 0; padding: 0; width: 2px;}
.navbar-section > li > a{padding: 0px 20px;}
.subnav-avatar{border: 2px solid white;  border-radius: 50%; height: 35px; width: 35px; display: inline-block; float: left; margin-top: 18px;}
.subnav-avatar img{border-radius: 50%; vertical-align: top;}
.subnav-avatar-down{display:inline-block; height: 74px;}
.subnav-avatar-down:hover{background: #1bb38c;}
.subnav-avatar-down > i{float: left; font-size: 1.2rem; margin-left: 15px; margin-top: 30px;}
.subnav-content{display: none; min-width: 340px; padding: 0; position: absolute; top: 74px; right: 0px; width: auto;background-color: #333333; line-height: 1.2;color:#fff;}
.subnav-content-box > .fl{margin-left:20px;}
.subnav-content-box .profile-menu-top-right span{font-size: 2rem; display:block; margin-top:10px;}
.subnav-content-box .profile-menu-top-right a{ font-size:1.4rem;} 
.subnav-content-box{ margin-top:2rem; border-bottom:1px solid #515151; height:7rem; margin-bottom:2rem;}
.subnav-content .subnav-link-item{ padding:15px 20px;}
.subnav-content .subnav-link-item span{ margin-left:20px;}
.subnav-content .subnav-link-item:hover{ background-color:#ff6700;}
/*未登录*/
.not-login{ width:204px;}
.not-login li{ width:100px; text-align:center;}
.not-login em{ display:block; border:1px solid #666; width:1px; height:50px; margin-top:11px;}
.not-login li,.not-login em{float:left;}
.not-login li a{ color:#808B8D; font-weight:900;}
.not-login li a:hover{ color:#fff;}
/*====main====*/
.main-wrapper{display:table; width: 100%; padding-top: 75px;}
.sidebar-container{display: table-cell;max-width: 250px; vertical-align: top; width: 250px; background: #f9f9f9 none repeat scroll 0 0; padding: 35px 0; height: 100%;}
.sidebar-container h2{ font-weight:100;}
.sidebar-container h2 a{ color:#3D3D3D;}
.icon-list {clear: both; color: #999999; font-size: 1.6rem; width: 100%; display: block; list-style: outside none none; margin-bottom: 1rem; margin-top: 1rem; position: relative; }
.icon-list a{ color:#999;}
.icon-list a:hover{ color:#1bb38c;}
.icon-list li {margin: 1.25em 0;}
.icon-list > li > i{color: #888888; line-height: 1.4; float: left; margin-right: 1.25em;}
.icon-list em{ display:block; float:left; width:120px;}
.icon-list em a{ display:inline-block;}
/*====main_right====*/
.content-container{display: table-cell; vertical-align: top;}
.my-story-container{margin: 0 auto; max-width: 1000px;}
.flex-video {height: 0; margin-bottom: 1rem; overflow: hidden; padding-bottom: 67.5%; padding-top: 1.5625rem; position: relative;  }
.flex-video .video-js {height: 100%;left: 0; position: absolute; top: 0; width: 100%; }
.media-item-description { padding: 0.5rem 1rem 1rem; font-size: 2.1rem; text-align:left;}
.media-item-description a,.media-item-description a:hover{ color:#3d3d3d; display:block; height:25.4rem;}
.media-item img{width: 100%;}
.activity-item li{padding:2px 6px; position: relative; cursor:pointer;}
.activity-item li.on p,.activity-item li:hover p{ background-color:#009999; color:#fff;}
.activity-item li p{border:1px solid #009999; padding: 6px; border-radius: 5px; text-align: center; color: #009999;}
.activity-item li span{position: absolute; right: 14px; top:21%; display: inline-block; color: #009999;font-size: 1.2rem; font-weight:bold;}
.activity-item li.on span,.activity-item li:hover span{color:#fff;}
section header {background: #3d3d3d none repeat scroll 0 0; color: white; padding: 0.5em 0.75em; position: relative;}
section header h3{display: inline-block; font-weight: bold;}
.portfolio-title{border-bottom: 1px solid #777777; margin-bottom: 1em; padding-bottom:.5em;}
.portfolio-age {float: right; margin-left: 2rem; font-weight: bold; margin-top: 1.3em; font-size:2rem;}
.media-item{background-color:#f9f9f9;}

/*footer{ background-color:#333; padding:4rem 0; text-align:center; color:#949494; line-height:1.6;}
img.bottom-logo{ width:19rem; height:19rem;}
img.bottom-kanwo{ width:19.5rem; height:4.2rem;}
footer a{ color:#949494;}
footer a:hover{ color:#ccc;}
footer p.pt40{ padding-bottom:4rem;}*/
/*新版底部*/
footer{text-align:center;padding:44px;background:#333;color:#fff;font-size:16px; line-height:1.8;}
footer a{ color:#1BB38C;}
footer a:hover{ color:#1DC79B;}

/*左侧信息栏*/
.inspiration-line{margin-bottom:.2rem; font-size:2rem; line-height:1.6; margin-bottom:1rem;}
.inspiration-image{width:100%;margin-bottom:1rem; width:17.5rem; height:25.8rem;}

h3.lead-in{border-left:8px solid #1bb38c;padding-left:.8rem;margin-bottom:.8rem; font-size:1.7rem; font-weight:900; letter-spacing:-.8px; height:3.6rem; line-height:3.6rem;}

.friend-impression-list{}
.friend-impression-list li{ padding:4px 8px; background:#f90; color:#fff; margin:5px; float:left; cursor:pointer; position:relative;}
.friend-impression-list .tips{ border-radius:10px; position:absolute; top:20px; left:26px; z-index:999; background:#f90; width:282px; display:none;}
.friend-impression-list .tips dl{ float:left; width:80px; margin:10px 0 0 10px; }
.friend-impression-list .tips dt img{ border-radius:50%; width:44px; height:44px; border:2px solid #fff;}
.friend-impression-list .tips dd{ width:100%; height:46px; margin-top:10px;}
.friend-impression-list .tips dd a{ border:1px solid #fff; display:inline-block; padding:2px 6px;}
.friend-impression-list .tips p{ line-height:3;} 
.friend-impression-list .tips dd a,.friend-impression-list .tips p{ color:#fff;}

/*编辑*/
.add-impression{ display:inline-block; padding:1rem 1.4rem; background-color:#009DD9; color:#fff; font-weight:900; border-radius:5px;-webkit-transition: all .2s ease-in-out;transition: all .2s ease-in-out; margin-bottom:1rem;}
.add-impression:hover{ background-color:#366699; color:#fff;}
.progress-section{ background-color:#F2F2F2; padding:1rem 2rem; margin-top:3rem;}
.progress-section h3{ font-size:3rem; font-weight:900; color:#515151; margin-bottom:2rem;}
.add-badges li{ float:left; width:10rem; height:10rem; margin:.46rem; margin-bottom:2rem;}

/*添加视频*/
.bg-gray{background-color:#F9F9F9;}
.add-my-video{ padding:1.5rem 0; letter-spacing:-.2px;}
.add-my-video h5{ font-size:1.6rem; margin-bottom:2rem;}
.add-video-description{ border-right:1px solid #c9c9c9; padding:0 5rem;}
.add-video-description p{ line-height:4; border-bottom:1px solid #C9C9C9; color:#5E5E5E;}
.add-video-description p.p-b-blue{ border-bottom:none; color:#879CD8;}
.add-video-btn{ display:inline-block !important; margin-left:13rem; padding:1rem 1.8rem; background-color:#1BB38C; color:#fff; font-weight:900; border-radius:5px;-webkit-transition: all .2s ease-in-out;transition: all .2s ease-in-out; margin-bottom:1rem;}
.add-video-btn:hover{ background-color:#009933; color:#fff;}
.add-my-video .md-trigger{ position:relative; margin-bottom:1rem; display:block;}
.add-my-video .md-trigger i{ position:absolute; font-size:6rem; color:#fff; top:50%; left:50%; margin-top:-2rem; margin-left:-6rem;}

.columns-info i{ color:#fff; font-size:4rem; margin-right:1rem; display:block !important; float:left; margin-top:1.6rem;}
.columns-info h6{ padding-left:2rem; background-color:#20bff0; height:6.6rem; line-height:6.6rem; border-radius:6px; border:1px solid #3399CC; color:#fff; font-weight:100; font-size:1.4rem;}
.columns-info h6 em{ display:inline-block;}
.columns-info .bg-gray{ text-align:center; margin:1rem 0;}
.columns-info .r-line{ border-right:1px solid #D7D7D7; height:26rem; margin:1.6rem 0; padding-top:6rem;}
.columns-info .bg-gray a.add-description,.columns-info .bg-gray a.add-upload,a.add-activity{ color:#aaa; font-size:1.6rem;}
.columns-info .bg-gray i,.columns-info a.add-activity i{ color:#aaa; font-size:8rem; float:none; margin-bottom:1rem;}
.columns-info .bg-gray i,.columns-info .bg-gray strong{ display:block !important; }
.columns-info .bg-gray strong,.add-activity-mb strong{ font-size:24px; letter-spacing:-.4px;font-weight:100;}
.columns-info .r-upload{ padding-top:8rem;}
.add-upload:hover i,.add-upload:hover strong,.add-activity:hover i,.add-activity:hover strong,.add-description:hover i,.add-description:hover strong {-webkit-transition: all .2s ease-in-out;transition: all .2s ease-in-out; color:#1BB38C !important;}
.add-activity-tit{ font-size:2.6rem; font-weight:900; border-bottom:1px solid #939393; line-height:6.4rem; height:6.rem; padding:0 2rem; color:#fff;}
.add-activity-tit,.add-activity-tit-edit{ background-color:#3D3D3D;}
.add-activity-tit-edit{ padding:.5rem 0;}
.add-activity-tit-edit i{ margin-top:0 !important;}
.add-activity-tit-edit i.glyphicon-triangle-top,.add-activity-tit-edit i.glyphicon-triangle-bottom{ color:#555; font-size:3rem; margin-left:2rem; border-right:1px solid #939393; padding-right:2rem;}
.add-activity-tit-edit .glyphicon-edit{ margin-top:.6rem !important; margin-left:1.4rem;}

.add-activity-mb{ margin-bottom:2rem;}

.bg-no{ background:none;}
.bg-no .r-upload{ background-color:#F9F9F9; padding:7rem 0;}

/*编辑*/
/*临时插入位置样式 统一rem px*/
.sortable-placeholder{width:322px; height:324px; border:1px solid red;float:left}

.media-item .r-upload{ padding-top:7rem !important; padding-bottom:7rem; background:#EBE9E9;}
.media-item .edit-hover,.media-item .show-hover{ position:relative; cursor:pointer; width:324px; height:324px; overflow:hidden;}
.media-item .controls{ position:absolute; top:0; right:0;opacity:0; width:100%; z-index:999;}
.media-item .controls a{ display:inline-block; border-radius:50%;box-shadow:0px 0px 25px rgba(0,0,0,0.3);width:3.2rem;height:3.2rem;text-align:center;margin:1rem 1rem 0 0;}
.media-item .controls i{ color:#fff; font-size:2rem;margin-right:0 !important;margin-top:.8rem !important;}
.media-item .controls a.controls-move{ margin-left:10px;}

.my-video-tips{ float:right; font-size:4rem; line-height:1.8;}
.video-img{ position:relative; text-align:center; height:26.99rem; line-height:26.99rem;}
.video-img i.glyphicon-play-circle{ position:absolute; z-index:2; left:50%; margin-top:-3rem; margin-left:-3rem; top:50%; font-size:8rem; color:#fff;}
.video-img .masking{ position:absolute; top:0; left:0; z-index:1; background:rgba(0,0,0,.4); width:100%; height:100%;}

.edit-my-video-info{ position:relative;}

.edit-my-video-info-btn{ display:block; position:absolute; top:0; right:0; cursor:pointer;}
.edit-my-video-info-btn i{ color:#ccc;}
.edit-my-video-info-content{ width:18rem; border:1px solid #ccc; background:#fff; position:absolute; right:32px; top:54px; z-index:99; display:none;}
.edit-my-video-info button.md-trigger{ color:#555; text-align:center; line-height:2 !important; font-size:2rem; display:block; border:none; background:none; width:100%;}
.edit-my-video-info button:hover{ background-color:#eee;}

.add-activity-tit-edit span{ display:inline-block; border:1px solid #009999; cursor:pointer; padding: .6rem; border-radius: 5px; color:#099; margin:1rem;}
.add-activity-tit-edit span:hover{ background-color:#099; color:#fff;}

.activity-list{ margin-bottom:3rem;}
.edit-document-item,.show-document-item{ background-color:#8AD480; color:#fff;padding-top:6rem; height:324px;}
.media-item .show-hover .edit-document-item{padding-top:8rem !important;}
.edit-document-item i.glyphicon-list-alt,.show-document-item i.glyphicon-list-alt{ font-size:7rem;color:#fff; }
.edit-document-item-title,.show-document-item-title{ font-size:2.6rem; margin:1.4rem 0; height:2.6rem; width:24.9rem; overflow:hidden; margin:0 auto;}
.edit-document-item small,.show-document-item small{ font-size:1rem;}
/*展开折叠*/
.show-height{ height:646px; overflow:hidden !important; width:100%;}
.btn-show{background:#20BFF0; width:100%; height:31px; line-height:31px; color:#fff; display:block; padding-left:40px;transition:background-color 300ms ease-out; -webkit-transition:background-color 300ms ease-out;}
.btn-show:hover{background:#0e9fcc !important;}

@media (max-width: 992px) {
    .sidebar-container{width:100% !important; max-width: 100% !important; display: block;}
    .content-container{ display: block;}
}




