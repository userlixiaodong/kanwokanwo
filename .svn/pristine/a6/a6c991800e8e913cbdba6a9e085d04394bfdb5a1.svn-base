@charset "utf-8";
* {
    margin: 0;
    padding: 0;
    font-family: 微软雅黑;
}

.clear {
    clear: both;
}

.gis_banner {
    width: 100%;
    height:455px;
    background: url(/images/CIS-banners.jpg) no-repeat center;
    margin-top: 74px;
    margin-bottom: 125px;
}

.mid_cont {
    width: 1300px;
    margin: 0 auto;
}

.enroll_box {
    width: 1300px;
    height: 1103px;
    background-color: #f3f6f9;
    margin-top: 46px;
    padding: 75px 55px;
    position: relative;
}

.enroll_box div:nth-child(1) {
    padding-left: 35px;
    padding-bottom: 158px;
}

.enroll_box div {
    position: relative;
}

.enroll_top p {
    width: 508px;
    position: absolute;
    top: 43px;
    right: 56px;
    font-size: 18px;
    font-family: "微软雅黑";
    color: #333333;
    line-height: 33px;
}

.enroll_top p span {
    font-size: 28px;
    font-family: "微软雅黑";
}

.enroll_bottom p {
    width: 560px;
    position: absolute;
    left: 40px;
    top: 36px;
    font-size: 28px;
    font-family: "微软雅黑";
}

.enroll_bottom p span {
    display: block;
    font-size: 18px;
    background: url(/images/CIS_06.png) no-repeat 1px 9px;
    height: 65px;
    padding-left: 54px;
    font-family: "微软雅黑";
    color: #333333;
}

.enroll_box .enroll_btn {
    height: 350px;
    width: 350px;
    background: url(/images/CIS_04.png) no-repeat center;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -175px;
    margin-top: -175px;
    text-align: center;
}

.enroll_btn a {
    display: block;
    width: 164px;
    height: 164px;
    background-color: #1e919e;
    border-radius: 100%;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -82px;
    margin-top: -82px;
    line-height: 164px;
    text-decoration: none;
    font-size: 29px;
    color: #FFFFFF;
    font-family: "微软雅黑";
    font-weight: bold;
    letter-spacing: 2px;
}

.enroll_btn a:hover {
    background-color: #2dafbe;
}

.application {
    height: 440px;
    margin: 0 auto;
    text-align: center;
}

.application p {
    margin: 98px 0 30px 0;
}


.project_details .btn {
    display: block;
    width: 206px;
    height: 54px;
    background-color: #29acbb;
    text-decoration: none;
    text-align: center;
    line-height: 54px;
    font-size: 28px;
    color: #FFFFFF;
    font-weight: bold;
    margin: 50px auto;
    border-radius: 63px;
    letter-spacing: 2px;
}

.project_details .btn:hover{
    background-color: #32c3d3;
}
.application p a{
    font-size: 31px;
    color: #333333;
    letter-spacing: 4px;
}

.application .consulting{
    width: 445px;
    height: 235px;
    border-left:1px dotted #666666;
    border-right:1px dotted #666666;
    margin: 0 auto;
    padding: 12px 0;
}
.application .consulting span{
    display: block;
    margin-top: 28px;
    font-size: 25px;
    color: #666;
    font-family: 微软雅黑;
    position: relative;
    letter-spacing: 9px;
}
.application .consulting span:before, .application .consulting span:after{
    display: block;
    content: "";
    width: 50px;
    border: 1px solid #999;
    margin-top: 20px;

}
.application .consulting span:before{
    margin-left:70px;
    float: left;
}
.application .consulting span:after{
    margin-right:70px;
    float: right;

}

.project_details {
    width: 1202px;
    height: 1100px;
    background-color: #f3f6f9;
    border-right: 9px solid #16a2b2;
    border-radius: 20px;
    margin:0 auto 30px auto ;
}

.project_details h2 {
    padding: 80px 0 50px 110px;
    color: #018c9c;
    font-size: 40px;
}

.project_details .contens {
    width: 974px;
    height: 768px;
    margin-left: 109px;
    background: url(/images/CIS_05.png) no-repeat 2px 1px;
}

.project_details .contens p {
    padding-left: 84px;
    padding-top: 23px;
    font-family: 微软雅黑;
}

.project_details .contens b {
    font-size: 18px;
    color: #333333;
    font-weight: normal;
    display: block;
    float: left;
    font-family: 微软雅黑;
}

.project_details .contens span {
    font-size: 18px;
    color: #666666;
    display: inline-block;
    padding-left: 22px;
    font-family: 微软雅黑;
    letter-spacing: 1px;
}

.project_details .contens i {
    font-style: normal;
    font-size: 18px;
    color: #999999;
}

.login-before{
    width: 700px;
    height: 340px;
    position: fixed;
    left: 50%;
    top: 50%;
    z-index:1000;
    text-align: center;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    background: #e1e4e8;
    display: none;
}

.login-before .login-before-close-btn{
    color: #aaaaaa;
    position: absolute;
    right: 10px;
    top: 10px;
}
.login-before p{
    font-family: "Microsoft Yahei";
    font-size: 18px;
    margin-top: 90px;
    width:100%;
    text-align: center;
}
.login-before .login-btn{
    display: block;
    width: 140px;
    height: 55px;
    font-size: 24px;
    line-height: 55px;
    color: #fff;
    text-align: center;
    margin: 70px auto;
    background-color: #28b38d;
}