/*CCTV媒体实习岗位start*/
.internship{
    width: 50%;
    margin: 4rem auto;
    height: auto;
}
.internship .titles{
    margin-bottom: 2rem;
}
.int_list {
    width: 94%;
    margin: 0 auto;
    list-style: none;
    text-align: left;
}

.int_list li {
    display: inline-block;
    list-style: none;
    width: 40%;
    height:39rem;
    /*height:auto;*/
    text-align: center;
    background-color: #f4f7fa;
    padding-left: 0;
    border-bottom: 0.3rem solid #20abbb;
    border-radius: 1rem;
}
.int_list li:nth-child(1){
    float: left;
}
.int_list li:nth-child(2){
    float: right;
}
.int_list li img{
    display: block;
    width: 100%;
}
.int_list li h6 {
    color: #1e919e;
    font-size: 1.4rem;
    font-weight: normal;
    margin-top: 2rem;
    font-family: 微软雅黑;
}

.int_list li p {
    margin: 0 auto;
    text-align: left;
    padding:1rem 1rem 2rem 1rem;
    line-height: 2rem;
    font-size: 0.7rem;
    color: #666666;
    text-align: justify;
    font-family: 微软雅黑;
}
.int_list li p span{
    display: block;
    margin-top:0rem;
    font-size:0.8rem;
}
.int_list li p span:first-child{
    margin-top:0;
}
/*.int_list li:nth-child(2) span{*/
    /*margin-top:2rem;*/
/*}*/
.practice-time{
    display: block;
    width:50%;
    height:auto;
    background: #f4f7fa;
    margin:3rem auto 0;
    padding:0.3rem 0.6rem;
    text-align: center;
    line-height:1.8rem;
    border-bottom: 0.25rem solid #20abbb;
    border-bottom-left-radius: 0.6rem;
    border-bottom-right-radius: 0.6rem;
    font-size:0.8rem;
}

@media (max-width: 1500px){
    .internship{
        width:80%;
    }
    @media (max-width: 1050px) {
        .internship{
            width:100%;
        }
        @media (max-width: 769px) {
            .internship{
                width: 90%;
            }
            .int_list{
                width: 92%;
            }
            .int_list li{
                width: 100%;
                height:auto;
            }
            .int_list li:nth-child(2){
                margin: 2rem 0;
            }
            .practice-time{
                width:80%;
            }
        }
    }
}

