    height: 2.75rem;
    font-size: 1.2rem;
    line-height: 2.75rem;
    color: #fff;
    text-align: center;
    margin: 3.5rem auto;
    background-color: #28b38d;
}
.leadFrame{
    display: none;
}

@media (max-width: 750px) {
    .login-before {
        height: 15rem;
        position: fixed;
        top: 30%;
        z-index: -99999;
        text-align: center;
        transform: translate(-50%, -50%);
        -webkit-transform: translate(-50%, -50%);
        background: #e1e4e8;
        display: none;
    }

    .login-before p {
        font-family: "Microsoft Yahei";
        font-size: 0.8rem;
        margin-top: 4.5rem;
        width: 100%;
        text-align: center;
    }

    .login-before .login-btn {
        display: block;
        width: 7rem;
        height: 2.75rem;
        font-size: 1.2rem;
        line-height: 2.75rem;
        color: #fff;
        text-align: center;
        margin: 3rem auto;
        background-color: #28b38d;
    }
    .glyphicon {
        position: absolute;
        top: 0.5rem;
        right: 0.5rem;
        display: inline-block;
        font-family: 'Glyphicons Halflings';
        font-style: normal;
        font-weight: normal;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        color: #fff;
    }
    .glyphicon-remove {
        font-size: 25px !important;
        display: block;
    }

    .mask {
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.6);
        z-index: 999;
        position: absolute;
        top: 0;
        left: 0;
        display: none;
    }
    .leadFrame{
        height: 15rem;
        position: fixed;
        top: 55%;
        z-index: 1000;
        text-align: center;
        transform: translate(-50%, -50%);
        -webkit-transform: translate(-50%, -50%);
        background: #1bb38c;
        display: none;
    }
    .leadFrame p{
        font-family: "Microsoft Yahei";
        font-size: 0.8rem;
        margin-top: 6.5rem;
        width: 100%;
        text-align: center;
        font-weight:bold;
        font-size:1.4rem;
        color:#fff;
        letter-spacing: 0.1em;
    }
}
