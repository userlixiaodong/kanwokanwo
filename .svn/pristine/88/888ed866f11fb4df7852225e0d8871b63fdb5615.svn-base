@charset "utf-8";
* {
    margin: 0;
    padding: 0;
}

html {
    font-size: 20px;
    font-family: 微软雅黑;
}

@media screen and (max-width: 1920px) {
    html {
        font-size: 18px !important;
    }
}

@media screen and (max-width: 1300px) {
    html {
        font-size: 16px !important;
    }
}

@media screen and (max-width: 1280px) {
    html {
        font-size: 14px !important;
    }
}

@media screen and (max-width: 750px) {
    html {
        font-size: 12px !important;
    }

    body {
        max-width: 750px;
    }
}
/* iphone6 plus */
@media (min-device-width: 414px) and (max-device-width: 736px) and (-webkit-min-device-pixel-ratio: 3) {
    html {
        font-size: 25.875px;
    }
}

/* iphone 6 */
@media (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    html {
        font-size: 23.4375px;
    }
}

/* 判断iphone5 *//* 横屏竖屏判断方法与ipad一样 */
@media only screen and (min-device-width: 320px) and (max-device-width: 568px) {
    html {
        font-size: 20px;
    }
}

.clear {
    clear: both;
}
/*新增样式end*/

.gis_banner {
    height: 25rem;
    background: url(/images/BA-ASRQ.jpg) no-repeat center top;
    background-size: 100% 25rem;
    margin-top: 3.7rem;
}
.gis_banner_btn {
    height: 3rem;
    position: absolute;
    text-align: center;
}

.signUp {
    display: inline-block;
    width: 8rem;
    height: 2.4rem;
    margin-top:1rem;
    background: #0cbc9d;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 2.4rem;
    color: #fff;
    font-family: "微软雅黑 Light";
    font-weight:bold;
    font-size:0.9rem;
    letter-spacing: 0.1em;
}

.signUp:nth-of-type(1) {
    /*float: left;*/
    /*margin-left: 6rem;*/
}

.signUp:nth-of-type(2) {
    float: right;
    margin-right: 6rem;
}
@media (max-device-width: 769px){
    .gis_banner {
        height: 15rem;
        background: url(/images/BA-ASRQ.jpg) no-repeat right top;
        background-size: 100% 15rem;
        margin-top: 5rem;
    }
    .gis_banner_btn {
        height: 3rem;
        position: absolute;
    }

    .signUp {
        display: inline-block;
        width: 6.4rem;
        height: 2.4rem;
        margin-top:-0.5rem;
        background: #018b9b;
        border-radius: 0.2rem;
        text-align: center;
        line-height: 2.4rem;
        color: #fff;
        font-family: "微软雅黑";
        font-weight:bold;
        font-size:0.8rem;
        letter-spacing: 0;
    }

    .signUp:nth-of-type(1) {
        /*float: left;*/
        margin-top: 0.5rem;
    }

    .signUp:nth-of-type(2) {
        float: right;
        margin-right: 3rem;
    }
    @media (max-width: 640px) {
        .gis_banner {
            height: 10rem;
            background: url(/images/BA-ASRQ.jpg) no-repeat -8rem top;
            background-size: cover;
            margin-top: 6rem;
        }
    }
}

/*项目背景*/
.project-bg{
    width:100%;
    height:auto;
    margin:4rem auto 0;
    padding:1rem 0;
}
.enroll_box {
    width: 70%;
    height: auto;
    background-color: #f3f6f9;
    margin: 2.3rem auto;
    padding: 3.75rem 2.75rem;
    position: relative;
}
