    z-index: 1000;
    text-align: center;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    background: #e1e4e8;
    display: none;
}

.login-before .login-before-close-btn {
    color: #aaaaaa;
    position: absolute;
    right: 0.5rem;
    top: 0.5rem;
}

.login-before p {
    font-family: "Microsoft Yahei";
