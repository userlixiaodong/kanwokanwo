}

@media (max-width: 750px) {
    .login-before {
        height: 15rem;
        position: fixed;
        top: 30%;
        z-index: -99999;
        text-align: center;
        transform: translate(-50%, -50%);
        -webkit-transform: translate(-50%, -50%);
        background: #e1e4e8;
        display: none;
    }

    .login-before p {
        font-family: "Microsoft Yahei";
        font-size: 0.8rem;
