}
.play{
    position: relative;
    top: -55%;
    left:50%;
    margin-left:-30px;
    display: inline-block;
    font-family: 'Glyphicons Halflings';
    font-style: normal;
    font-weight: normal;
    font-size: 60px;
    color: #fff;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
}
.play:before{
    content: "\e029";
    box-sizing: border-box;
}

