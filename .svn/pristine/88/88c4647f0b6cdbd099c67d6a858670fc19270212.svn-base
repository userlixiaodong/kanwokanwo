.videoIntroduce{
    width:100%;
    height:38rem;
    position: relative;
    margin:4rem auto;
    padding:2rem 0 4rem 0;
    overflow: hidden;
    background: url(/images/video-banner.png) no-repeat center center;
}
.videoWrapper{
    width: 40%;
