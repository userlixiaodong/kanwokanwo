<script type="text/javascript">
    var wid = $(window).width();
    var hei = $(window).height();
    $(function () {
        $(window).load(function () {
            $(".loaded").fadeOut();
            $(".preloader").delay(1000).fadeOut("slow");
        });
        //转学保障部分切换控制
        var guaranteeLoopLen = $(".guarantee-list").length; //轮播图总数
        console.log(guaranteeLoopLen);
        var loopWid = $(".loop").width();
        console.log(loopWid);
        var step = 1; 	//每一步移动的图片数 ul个数
        //左箭头点击处理
        $("#prev").click(function(){
            //首先，获得当前ul的marginLeft
            console.log(currentPos);
            var currentPos = parseFloat($(".guarantee-left-loop").css('marginLeft'));
            //计算出前一页ul的marginLeft
            var prevPos = (currentPos+loopWid)>0?0:(currentPos+loopWid);
            console.log(prevPos);
            $(".guarantee-left-loop").animate({marginLeft: prevPos}, 600);
            // $(".guarantee-list").eq(1).prependTo($(".guarantee-left-loop"));
            $(".guarantee-left-loop").css('marginLeft',prevPos);
        });
        //右箭头点击处理
        $("#next").click(function(){
            var currentPos = parseFloat($(".guarantee-left-loop").css('marginLeft'));
            console.log(currentPos);
            var nextPos = (currentPos-loopWid)<(guaranteeLoopLen-step)/step*(-loopWid)?(guaranteeLoopLen-step)/step*(-loopWid):(currentPos-loopWid);
            console.log(nextPos);
            console.log((guaranteeLoopLen-step)/step*(-loopWid));
            $(".guarantee-left-loop").animate({marginLeft:nextPos}, 600, function () {
                // $(".guarantee-list").eq(0).appendTo($(".guarantee-left-loop"));
                $(".guarantee-left-loop").css('marginLeft', nextPos);
            });
        });
        //成功案例添加背景图片
        var advList = $(".advantage ul li");
        for (var i = 0; i < advList.length ; i++) {
            var advListUrl = '/images/adv'+parseInt(i+1)+'.png';
            advList.eq(i).css("background","url("+advListUrl+") no-repeat center 30px #f5f8fa");
        }
        //成功案例图像对应内容切换
        $(".charactersImg li").on("click",function(){
            var i = $(this).index();
            var chaSpanList = $(".characters span").eq(i);
            $(this).css("border","2px solid #fff").children("span").css("display","none");
            $(this).siblings("li").css("border","none").children("span").css("display","block");
            chaSpanList.css("display","block").siblings().css("display","none");
        });

        $(".gis_banner_btn").css({"width":wid*0.4,"top":$(".gis_banner").height()*0.8,"left":wid*0.3});
        if (wid < 769){
            $(".mid_cont").css({"width":wid,"max-width":"750px","margin":"0 auto"});
            $(".gis_banner").css("width",wid);
            $(".gis_banner_btn").css({"width":wid,"top":$(".gis_banner").height()*0.7,"left":0});
            $(".project_details").css({"width":wid*0.98,"margin-left":wid*0.01});
            $(".leadFrame").css({"width":wid*0.8,"left":wid*0.5});
            $(".user-login").css({"width":wid,"height":hei,"position":"fixed","margin-left":wid*0.1,"top":"0px"});
            $(".loop").css("width",wid*0.9);
            $(".guarantee-left-loop").css("width",wid*0.9*2);
            console.log($(".loop").width());
            console.log($(".guarantee-left-loop").width());
        }
    })

    function loginAlert(){
        if (wid < 769){
            $("#signUp,.videoSignUp").on("click",function () {
                $(".mask").css("display","block").bind("touchmove",function(e){
                    e.preventDefault();
                    e.stopPropagation();
                },false);
                $(".leadFrame").show(400).bind("touchmove",function(e){
                    e.preventDefault();
                    e.stopPropagation();
                },false);
            });
            $(".login-before-close-btn").on("click",function () {
                $(".mask").css("display","none");
                $(".leadFrame").css("display","none");
            });
        }else {
            $(".login-before").css('display','block');
        }
    }
    function closeAlert(){
        $(".login-before").css('display','none');
        location.reload();
    }
