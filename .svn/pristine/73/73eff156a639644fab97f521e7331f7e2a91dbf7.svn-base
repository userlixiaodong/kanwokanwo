@charset "utf-8";
* {
    margin: 0;
    padding: 0;
    font-family: 微软雅黑;
}

html {
    font-size: 20px;
}
body{
    background: #f3f3f3;
}

@media screen and (max-width: 1920px) {
    html {
        font-size: 18px !important;
    }
}

@media screen and (max-width: 1300px) {
    html {
        font-size: 16px !important;
    }
}

@media screen and (max-width: 1280px) {
    html {
        font-size: 14px !important;
    }
}

@media screen and (max-width: 750px) {
    html {
        font-size: 12px !important;
    }

    body {
        max-width: 750px;
    }
}
/* iphone6 plus */
@media (min-device-width: 414px) and (max-device-width: 736px) and (-webkit-min-device-pixel-ratio: 3) {
    html {
        font-size: 25.875px;
    }
}
/* iphone 6 */
@media (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    html {
        font-size: 23.4375px;
    }
}
/* 判断iphone5 *//* 横屏竖屏判断方法与ipad一样 */
@media only screen and (min-device-width: 320px) and (max-device-width: 568px) {
    html {
        font-size: 20px;
    }
}

.clear {
    clear: both;
}

/*banner图start*/
.gis_banner {
    height: 25rem;
    margin-top: 3.7rem;
    position: relative;
}
.ban-pic{
    width:100%;
    height:100%;
}
.ban-wx{
    position: absolute;
    top:5rem;
    right:8rem;
}
.gis_banner_btn {
    height: 3rem;
    position: absolute;
    text-align: center;
    bottom:2.5rem;
    left:34%;
}

.registerBtn {
    display: inline-block;
    width: 8rem;
    height: 2.4rem;
    background: url("/images/free-register.png") no-repeat;
    background-size:cover ;
    text-indent: -99999;
}

@media (max-device-width: 769px){
    .gis_banner {
        height: 15rem;
        margin-top: 5rem;
    }
    .ban-pic{
        width:100%;
        height:100%;
    }
    .ban-wx{
        width:6rem;
        height:auto;
        position: absolute;
        top:3rem;
        right:5rem;
    }
    .gis_banner_btn {
        height: 3rem;
        position: absolute;
    }
    .gis_banner_btn {
        bottom:0.5rem;
        left:30%;
    }

    @media (max-device-width: 640px) {
        .gis_banner {
            height: 10rem;
            margin-top: 6rem;
        }
        .ban-wx{
            width:6rem;
            height:auto;
            position: absolute;
            top:1rem;
            right:1.2rem;
        }
        .gis_banner_btn {
            bottom:0;
            left:24%;
        }
        .registerBtn {
            width: 7rem;
            height: 2.4rem;
        }
    }
}
.mid_cont{
    width:100%;
    height:auto;
    margin:0 auto;
    overflow: hidden;
    background: #f3f3f3;
}
/*part1现在，视频简历可以用于申请所有的美国大学start*/
.part1{
    width:100%;
    height:auto;
    background: url("/images/videoProfile-part1-bg.png") no-repeat right top #fff;
    background-size: 60% 100%;
    padding:4rem 0;
}
.part1-wrapper{
    width:80%;
    height:auto;
    overflow: hidden;
    margin:0 auto;
}
.part1-text{
    width:50%;
    height:auto;
    float: left;
}
.part1-title li{
    list-style-type: square;
    width:100%;
    height:2rem;
    color: #08b08f;
    line-height:2rem;
}
.part1-title li b{
    color: #08b08f;
    font-size:1.2rem;
}
.texts{
    width:80%;
    height:auto;
    background:url(/images/videoProfile-dot.png) no-repeat 80% 10%;
    background-size:22% ;
    margin-top:2rem;
    position: relative;
    text-align: center;
}
.texts img{
    width:12%;
}
.texts p{
    display: block;
    font-size:0.8rem;
    line-height:1.4rem;
    padding-left:0;
    text-align: left;
}
.texts p:nth-of-type(1){
    font-style: italic;
    color: #809792;
}
.texts p:nth-of-type(2){
    margin-top:1rem;
    font-family:"微软雅黑";
    color: #333;
}
.interview-describe{
    width:100%;
    height:6rem;
    background: url("/images/videoProfile-textBg.png") no-repeat center top;
    background-size:70%;
    text-align: center;
    font-size: 0.8rem;
    font-family: "微软雅黑";
    font-weight: bold;
    border-bottom: 1px solid #fff;
    padding-top:1.5rem;
}
.interview-describe span:nth-of-type(1){
    color: #01896e;
    font-family:"Arial";
    font-size:0.8rem;
    margin-right:1rem;
}
.interview-describe span:nth-of-type(2){
    color: #333;
    font-family:"微软雅黑";
    font-size:0.7rem;
    border-left:1px solid #999;
    padding-left:1rem;
}
.part1-video{
    width:50%;
    height:auto;
    float: right;
}
.colleges-column {
    margin-top:2rem;
}
.colleges-content {
    color: #fff;
    top: 0;
    left: 0;
    height: 100%;
    position: absolute;
}
.colleges-content dl {
    bottom: 0;
    left: 50%;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
}

.colleges-content dt {
    font-size: 0.9rem;
    font-weight: 100;
    line-height: 1rem;
}

.colleges-content dd {
    font-size: 0.7rem;
    line-height: 1rem;
}
.colleges-content dd a, .colleges-btn {
    font-size: 2.5rem;
    color:rgba(255,255,255,0.6);
    display: inline-block;
    margin-bottom: 0;
}
.colleges-content dd {
    width: 100%;
}
.colleges-content dd img {
    border: 0.1rem solid #fff;
}
.colleges-btn {
    display: none;
}
@media (max-width: 769px) {
    .part1-wrapper{
        width:90%;
    }
    .part1-text{
        text-align: center;
    }
    .texts{
        margin-left:2rem;
    }
    .part1-video{
        width:50%;
        height:23rem;
    }
    .part1-video img{
        height:100%;
    }
    @media (max-width: 640px) {
        .part1{
            background: url(/images/videoProfile-part1-bg.png) no-repeat center top #fff;
            background-size: 150% 30%;
        }
        .part1-wrapper{
            width:96%;
        }
        .part1-text{
            float: none;
            width:100%;
        }
        .texts img{
            width:20%;
        }
        .interview-describe{
            background-size: cover;
        }
        .part1-video{
            float: none;
            width:100%;
        }
    }
}
/*part1现在，视频简历可以用于申请所有的美国大学end*/
/*part2视频简历——让你在招生官眼中更鲜活start*/
.part2{
    width:100%;
    height:auto;
    padding:4rem 0;
    margin:0 auto;
    overflow: hidden;
    background: url("/images/videoProfile-part2-bg.png") no-repeat;
    background-size:cover ;
}
.part2-wrapper{
    width:80%;
    height:auto;
    margin:2rem auto 0;
    overflow: hidden;
}
.part2-video {
    width: 48%;
    float: left;
}
.part2-btn dl{
    width:100%;
    height:100%;
    position: relative;
}
.part2-btn dd a{
    font-size: 2.6rem;
    color:#18cf9e;
    display: inline-block;
    margin-bottom: 0;
    position: absolute;
    top:50%;
    left:50%;
    margin-left:-1.3rem;
    margin-top:-1.3rem;
}
.video-introduce-left{
    background: url("/images/videoProfile-student.png") no-repeat left center;
    background-size:8% ;
    width:90%;
    float: left;
    padding:0 0 0 1rem;
    font-size:0.9rem;
    line-height:1.4rem;
    margin:1rem 0 0 2rem;
}
.video-introduce-left p{
    margin-left:3rem;
    font-size:0.8rem;
    font-family:"Arial";
    padding-left:0;
}
.video-introduce-left p:nth-of-type(1){
    color: #18cf9e;
    font-weight:bold;
}
.video-introduce-left p:nth-of-type(2){
    color: #fff;
    font-style: italic;
}
.video-description{
    float: right;
    margin-right:0;
}
.part2-students{
    width:45%;
    height:auto;
    float: right;
    padding:2rem 0;
}
.part2-students li{
    width:100%;
    height:8rem;
    padding:0.6rem 0 0.6rem 4rem;
    overflow: hidden;
    margin-top:0.5rem;
}
.part2-students li:nth-of-type(1){
    background: url("/images/videoProfile-student1.png") no-repeat;
    margin-top:0;
}
.part2-students li:nth-of-type(2){
    background: url("/images/videoProfile-student2.png") no-repeat;
}
.part2-students li:nth-of-type(3){
    background: url("/images/videoProfile-student3.png") no-repeat;
}
.video-introduce{
    width:70%;
    float: left;
    padding:0.8rem 0 0 1rem;
    font-size:0.9rem;
    margin-left:2rem;
}
.video-introduce p{
    height:auto;
    padding-left:0;
    color: #fff;
    line-height:2rem;
    font-size:0.8rem;
}
.video-introduce p:nth-of-type(1){
    font-weight:bold;
    font-style: italic;
}
.hisVideo{
    display: inline-block;
    width:7.4rem;
    height:2rem;
    background: #0aaa82;
    border-radius: 16rem;
    box-shadow: 0 0 0.1rem #fff;
    color: #fff;
    line-height:2rem;
    text-align: center;
    font-size:0.7rem;
    float: right;
    font-style: normal;
    font-weight:normal;
    letter-spacing: 1px;
}
.underLine{
    width: 5.4rem;
    height:0.12rem;
    background: #0aaa82;
    margin: 0.8rem auto 0;
}
@media (max-width: 769px) {
    .part2-wrapper{
        width:96%;
    }
    .part2-video{
        height:28rem;
    }
    .colleges-column{
        height:70%;
    }
    .part2-video img{
        height:100%;
    }
    .part2-btn dd a{
        font-size:3rem;
    }
    .video-introduce-left{
        background-size: 18%;
        margin:1rem 0 0 0rem;
    }
    .hisVideo{
        margin-right:-3rem;
    }
    .part2-students{
        width:50%;
    }
    @media (max-width: 640px) {
        .part2-video{
            float: none;
            width:100%;
        }
        .part2-students{
            float: none;
            width:100%;
        }
        .part2-students li{
            height:12rem;
        }
        .video-introduce-left{
            padding:0 0 0 2.4rem;
        }
        .video-introduce{
            padding:0.8rem 0 0 2.5rem;
        }
    }
}
/*part2视频简历——让你在招生官眼中更鲜活end*/
/*part3从七年级开始，记录你的活动和成就，以便在申请学校时做出系统性的展现start*/
.part3{
    width:100%;
    height:auto;
    background: url("/images/videoProfile-part3-leftBg.png") no-repeat 7rem 2rem #fff;
    background-size: 40%;
    overflow: hidden;
    margin:0 auto;
    padding:4rem 0;
}
.part3-wrapper{
    width:80%;
    height:auto;
    margin:0 auto;
    overflow: hidden;
}
.part3-left{
    width:48%;
    float: left;
    margin:4rem 0;
    text-align: right;
}
.part3-left img{
    width:80%;
}
.part3-text{
    width:46%;
    float: left;
    text-align: center;
    margin:4rem auto;
    padding:0 2rem;
}
.texts3{
    width:100%;
    height:auto;
    background:url(/images/info-2-student.jpg) no-repeat left top;
    padding-left:6rem;
    margin-top:2rem;
    position: relative;
    text-align: left;
}
.part3-text h3{
    text-align: left;
}
@media (max-width: 769px) {
    .part3{
        padding:4rem 0 0;
    }
    .part3-wrapper{
        width:96%;
    }
    .part3-left{
        height:26rem;
    }
    .part3-left img{
        width: 100%;
        height: 80%;
        margin-top: 3rem;
    }
    .part3-text{
        width:50%;
        padding:0;
    }
    .part3-text li{
        text-align: left;
    }
    @media (max-width: 640px) {
        .part3{
            background: url(/images/videoProfile-part3-leftBg.png) no-repeat -5rem 0 #fff;
            background-size: 80%;
        }
        .part3-left{
            float: none;
            width:100%;
            height:22rem;
        }
        .part3-left img{
            width:100%;
            height:100%;
            margin:0;
        }
        .part3-text{
            float: none;
            width:100%;
            margin:2rem auto 0;
        }

    }
}
/*part3从七年级开始，记录你的活动和成就，以便在申请学校时做出系统性的展现end*/
/*part4一份独具特色的视频简历是如何诞生的start*/
.part4{
    width:100%;
    height:auto;
    overflow: hidden;
    margin:2rem auto 0;
    padding:2rem 0 6rem;
}
.howToBorn{
    width:70%;
    height:auto;
    margin:5rem auto 0;
    padding:0;
    overflow: hidden;
}
.howToBorn li{
    width:25%;
    height:auto;
    float: left;
    margin:0;
    padding:0;
    vertical-align: top;
}
.howToBorn li img{
    width:100%;
    height:auto;
}
.howToBorn li span{
    display: block;
    width:100%;
    height:5rem;
    font-size:0.8rem;
    text-align: left;
    line-height:1.6rem;
    padding:0.8rem;
    background: #fff;
}
@media (max-width: 769px) {
    .howToBorn{
        width:90%;
    }
    @media (max-width: 640px) {
        .howToBorn{
            width:98%;
        }
        .howToBorn li{
            width:50%;
        }
    }
}
/*part4一份独具特色的视频简历是如何诞生的end*/
/*part5底部模块start*/
.part5{
    width:100%;
    height:28rem;
    margin:0 auto;
}
.part5-video{
    width:55%;
    height:28rem;
    float: left;
    background: #0aaa82;
    padding:0;
    position: relative;
}
.part5-video img{
    width:100%;
    height:100%;
}
.part5-right{
    width:45%;
    height:28rem;
    float: right;
    background: url("/images/videoProfile-part5-bg.jpg") no-repeat;
    background-size:cover ;
    position: relative;
}
.part5-rightInner{
    width:60%;
    height:60%;
    position: absolute;
    top:20%;
    left:-0.8rem;
    overflow: hidden;
}
.part5-list{
    width:70%;
    height:auto;
    float: left;
}
.part5-list li{
    list-style-type: none;
    background: url(/images/videoProfile-zan.png) no-repeat left center;
    background-size: 8%;
    width:100%;
    height:2.5rem;
    line-height:2.5rem;
    padding-left:3.5rem;
}
.part5-list li:nth-of-type(5){
    background: none;
}
.part5-list li b{
    color: #fff;
    font-size:0.9rem;
    font-weight:normal;
    font-family:"微软雅黑 Light";
}
.part5-list li a{
    display: block;
    width:9rem;
    height:2rem;
    background: #0fc594;
    border-radius: 1rem;
    color: #fff;
    font-size:0.7rem;
    text-align: center;
    line-height:2rem;
    margin:1rem 0 0 -2rem;
}
.part5-erweima{
    width:30%;
    height:70%;
    float: right;
    text-align: center;
    padding:1.7rem 0;
    border:1px solid #fff;
    margin-top:1rem;
}
.part5-erweima img{
    width:70%;
}
.part5-erweima p{
    width:100%;
    height:2rem;
    font-size:0.8rem;
    line-height:2rem;
    margin-top:1rem;
    color: #fff;
    font-family:"微软雅黑";
}
@media (max-width:1660px ) {
    .part5-rightInner{
        width:70%;
    }
    @media (max-width: 1425px) {
        .part5-rightInner{
            width:80%;
        }
        @media (max-width: 1088px) {
            .part5-rightInner{
                width:90%;
            }
        }
    }
}
@media (max-width: 769px) {
    .part5,.part5-video,.part5-right{
        height:20rem;
    }
    .part5-rightInner{
        height:80%;
        top:17%;
    }
    .part5-list li{
        background-size: 12%;
    }
    .part5-list{
        width:60%;
    }
    .part5-erweima{
        width:40%;
    }
    @media (max-width: 640px) {
        .part5, .part5-video, .part5-right{
            height:40rem;
        }
        .part5-video{
            float: none;
            width:100%;
            height:50%;
        }
        .part5-right{
            float: none;
            width:100%;
        }
        .part5-rightInner{
            height:50%;
            width:80%;
            left:10%;
            top:0;
            padding:1.8rem 0;
        }
        .part5-list li a{
            width:12rem;
        }
        .part5-list li:nth-of-type(5){
            padding-left:2rem;
        }
    }
}
/*part5底部模块start*/

/*标题*/
.titles{
    width:100%;
    height:auto;
    text-align: center;
    line-height:2rem;
    font-size:1.6rem;
    font-weight:400;
    color: #333;
    letter-spacing: 1px;
}
/*标题下划线*/
.toLine{
    border-bottom:0.15rem solid #17ac84;
    padding-bottom:0.6rem;
    font-weight:normal;
}
@media (max-width: 769px) {
    .underLine{
        display: block;
    }
    .titles{
        width:86%;
        margin:0 auto;
        line-height:2rem;
        font-size:1.2rem;
    }
}