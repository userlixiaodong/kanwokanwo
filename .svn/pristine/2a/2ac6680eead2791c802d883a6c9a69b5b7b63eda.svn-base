/*学院特色*/
.feature{
    width:100%;
    height:auto;
    margin:4rem auto 0;
    padding:2rem 0;
}
.feature ul{
    list-style-type: none;
    width:80%;
    height:auto;
    margin:4rem auto;
    text-align: center;
}
.feature ul li{
    display: inline-block;
    width: 18%;
    height:auto;
    vertical-align: top;
}
.feature-scroll{
    border: 1px solid #ddd;
    width: 4rem;
    height: 4rem;
    line-height: 4rem;
    margin: 1rem auto;
    transition: all 0.6s;
}
.feature-scroll i{
    color: #1bb38c;
    transition: all 0.6s;
}
.fa{
    display: inline-block;
    transform: translate(0, 0);
}
.fa-clone {
    width:1rem;
    height:1rem;
    background: url("/images/fa-clone.png") no-repeat;
    background-size:cover ;
    margin-top:1.5rem;
}
.fa-heart{
    width:1rem;
    height:1rem;
    background: url("/images/fa-heart.png") no-repeat;
    background-size:cover ;
    margin-top:1.5rem;
}
.fa-lightbulb{
    width:1.4rem;
    height:1.4rem;
    background: url("/images/fa-lightbulb.png") no-repeat center top;
    background-size:cover;
    margin-top:1.2rem;
}
.fa-comments{
    width:1rem;
    height:1rem;
    background: url("/images/fa-comments.png") no-repeat;
    background-size:cover ;
    margin-top:1.5rem;
}
.fa-people{
    width:1rem;
    height:1rem;
    background: url("/images/fa-people.png") no-repeat;
    background-size:cover ;
    margin-top:1.5rem;
}
.feature h4{
    font-size:0.9rem;
    transition: all 0.6s;
    color: #999;
    font-family:"微软雅黑";
}
.separator{
    height: 2px;
    width: 40px;
    margin: 20px auto;
    background: #888888;
}
.feature-introduce{
    width:80%;
    margin:0 auto;
    line-height: 2rem;
    font-size: 0.7rem;
    text-align: center;
    color: #666;
    font-family:"微软雅黑";
}
.feature-introduce:last-child{
    margin-bottom: 0px;
}
.feature ul li:hover .feature-scroll{
    border-color:#1bb38c;
    transform: rotate(45deg);
}
.feature ul li:hover .feature-scroll i{
    transform: rotate(-45deg);
}
.feature ul li:hover h4{
    color:#1bb38c;
}
@media (max-width: 768px) {
    .feature ul li{
        display: block;
        width:100%;
        position: relative;
        min-height: 1px;
        padding-right: 0.8rem;
        padding-left: 0.8rem;
        margin:4rem auto;
    }
    .feature-scroll{
        width: 6rem;
        height: 6rem;
        line-height: 6rem;
        margin: 1rem auto;
        transition: all 0.6s;
    }
    .fa-clone {
        width:1.4rem;
        height:1.4rem;
        margin-top:2.5rem;
    }
    .fa-heart{
        width:1.4rem;
        height:1.4rem;
        margin-top:2.4rem;
    }
    .fa-lightbulb{
        width:2rem;
        height:1.8rem;
        margin-top:2.2rem;
    }
    .fa-comments{
        width:1.4rem;
        height:1.4rem;
        background: url("/images/fa-comments.png") no-repeat;
        background-size:cover ;
        margin-top:2.5rem;
    }
    .fa-people{
        width:1.4rem;
        height:1.4rem;
        background: url("/images/fa-people.png") no-repeat;
        background-size:cover ;
        margin-top:2.5rem;
    }
    .feature-introduce{
        line-height:2rem;
        font-size:1rem;
    }
}

/*从Lanpa到名牌大学*/
.lanpa{
    width:100%;
    height:auto;
    margin:0 auto 4rem;
    color: #fff;
}
.container-fluids{
    padding-right: 0.8rem;
    padding-left: 0.8rem;
    margin:3rem auto;
}
.container-fluids:before{
    display: table;
    content: " ";
}
.lanpa-row{
    margin-right: -0.8rem;
    margin-left: -0.8rem;
}
.lanpa-row:before{
    display: table;
    content: " ";
}
.lanpa-left{
    width:50%;
    height:25rem;
    float: left;
}
.lanpg-right{
    width:50%;
    height:36rem;
    float: left;
}
.lanpg-right img{
    width:100%;
    height:100%;
}
.no-padding {
    padding: 0 !important;
    margin: 0 !important;
}
.lanpa-text{
    margin-right: 0px;
    background: #202020;
    text-align: right;
}
.head-title{
    padding: 4rem 0 2rem 0;
    margin-top: 0rem;
}
.head-title h2{
    margin-right: 12%;
    border-right: 6px solid #1bb38c;
    color: #fff;
    padding-right: 5%;
    font-family: 'Source Sans Pro', sans-serif,'微软雅黑';
    font-size: 1.4rem;
    line-height: 2rem;
    text-transform: uppercase;
    font-weight: 700;
}
.lanpa-row li{
    width:100%;
    height:7rem;
}
.lanpa-row-inner{
    width:90%;
    height:100%;
    padding-right:1.8rem;
}
.lanpa-row-inner h4{
    color: #1bb38c;
    font-family:'微软雅黑', 'Open Sans', sans-serif;
    font-weight: 600;
    font-size: 1rem;
    line-height: 2rem;
    margin-bottom: 1rem;
    text-transform: uppercase;
}
.lanpa-row-inner p{
    color: #999999;
    font-family: '微软雅黑','Source Sans Pro', sans-serif;
    font-weight: 400;
    line-height: 2rem;
    font-size: 0.7rem;
    margin: 0 0 0.7rem;
}
.lanpa-row-inner p:last-child{
    margin-bottom: 0;
}
.lanpa-heart{
    width:5rem;
    height:5rem;
    float: right;
    margin-top:-7rem;
    position: relative;
    min-height: 1px;
    padding-right: 0.8rem;
    padding-left: 0.8rem;
}
figure{
    display: block;
    margin:0;
}
figure i {
    background: url("/images/figure-i.png") no-repeat #f4f5f9 center center;
    background-size: 60%;
    width: 4rem;
    height: 4rem;
    line-height: 4rem;
    text-align: center;
    color: #e74c3c;
    position: relative;
    z-index: 9;
    border: 5px solid rgba(0, 0, 0, 0.2);
    border-radius: 50%;
    font-size: 1.875rem;
    margin: 0.34rem -0.5rem;
}
@media (max-width: 769px) {
    /*.container-fluids{*/
        /*padding-right: 0rem;*/
        /*padding-left: 0rem;*/
    /*}*/
    .lanpa-row-inner p{
        line-height:1.4rem;
    }
}
@media (max-width: 640px) {
    .container-fluids{
        padding-right: 0rem;
        padding-left: 0rem;
    }
    .head-title h2{
        margin-right: 8%;
        line-height: 3rem;
    }
    .lanpa-row{
        width:100%;
        margin:0 auto;
    }
    .lanpa-left{
        width:100%;
        height:auto;
        float: none;
    }
    .lanpg-right {
        width:100%;
        float: none;
    }
    .lanpg-right img{
        width:100%;
    }
    .lanpa-text{
        padding:2rem 0 8rem;
    }
    .lanpa-row li {
        width:90%;
        height: 8rem;
        margin-top:4rem;
        padding-left:3rem;
    }
    .lanpa-row li:nth-of-type(1){
        margin-top:0;
    }
    .lanpa-heart {
        width: 8rem;
        height: 8rem;
        margin-right:-3rem;
        margin-top:-8rem;
    }
    figure i {
        background-size: 80%;
        width: 5rem;
        height: 5rem;
        line-height: 5rem;
    }
    .lanpa-row-inner p{
        line-height:2.4rem;
        font-size:1rem;
        padding-right:0;
    }
}

/*转学保障*/
.guarantee{
    width:100%;
    height:auto;
    background: #f4f5f9;
    margin:6rem auto;
    padding:2rem 0;
}
.guarantee-container{
    width:60%;
    height:auto;
    padding-right: 0.8rem;
    padding-left: 0.8rem;
    margin:2rem auto;
    text-align: center;
}
@media (max-width: 1578px) {
    .guarantee-container{
        width:80%;
    }
}
@media (max-width: 1550px) {
    .guarantee-left{
        width:28rem;
        height:auto;
        top:2rem;
    }
}
@media (max-width: 1308px) {
    .guarantee-container{
        width:80%;
    }
}
@media (max-width: 945px) {
    .guarantee-container{
        width:90%;
    }
}
@media (max-width: 769px) {
    #prev,#next{
        display: none;
    }
}
.guarantee-container:before{
    display: table;
    content: " ";
}
.guarantee-description{
    width:50%;
    height:auto;
    margin:0 auto;
    font-size:0.7rem;
    text-align: center;
    line-height:1.6rem;
    color: #999;
    font-family:"微软雅黑";
}
.separator-round{
    width: 0.6rem;
    height: 0.6rem;
    margin: 1rem auto;
    background: #1bb38c;
    position: relative;
    display: inline-block;
    border-radius: 50%;
}
.separator-round:before{
    position: absolute;
    right: 5%;
    bottom: 0.2rem;
    height: 0.05rem;
    width: 4rem;
    background: #ddd;
    content: "";
    display: inline-block;
    margin-right: 1rem;
}
.separator-round:after{
    position: absolute;
    left: 5%;
    bottom: 0.2rem;
    height: 0.05rem;
    width: 4rem;
    background: #ddd;
    content: "";
    display: inline-block;
    margin-left: 1rem;
}
.guarantee-content{
    width:100%;
    height:auto;
    position: relative;
    margin:5rem auto;
}
.guarantee-left{
    width:32rem;
    height:auto;
    background: #fff;
    position: absolute;
    left:3rem;
    top:4rem;
    padding:0 1rem;
}
.guarantee-left-wrapper{
    width:28rem;
    height:100%;
    margin:0 auto;
    position: relative;
    overflow: hidden;
}
.guarantee-left-loop{
    width:56rem;
    height:100%;
    margin-left:0;
}
.loop{
    width:28rem;
    height:32rem;
    float: left;
    background: #fff;
    margin:0;
}
.guarantee-icon{
    /*background: #1bb38c;*/
    width: 3.2rem;
    height: 3.2rem;
    line-height: 3.2rem;
    color: #fff;
    transform: rotate(45deg);
    margin: 2.5rem auto 0;
    border:1px solid #1bb38c;
}
.guarantee-icon i{
    width:1.4rem;
    height:1.4rem;
    background: url("/images/guarantee-icon-i.png") no-repeat center top;
    background-size:cover;
    margin-top:0.9rem;
    transform: rotate(-45deg);
}
.loop p{
    font-family: '微软雅黑','Open Sans', sans-serif;
    font-weight: 600;
    font-size: 0.86rem;
    line-height: 2rem;
    margin: 1.6rem auto 0;
    text-transform: uppercase;
    color: #797979;
}
.separator3{
    height: 0.1rem;
    width: 2rem;
    margin: 1rem auto;
    background: #888888;
}
.guarantee-list{
    display: inline-block;
    width:100%;
    height:auto;
    vertical-align: top;
    text-align: center;
}
.guarantee-list > li{
    width:100%;
    height:auto;
    line-height:2rem;
    margin:0 auto;
    text-align: center;
    color: #1bb38c;
}
.guarantee-text{
    font-size:0.6rem;
    color: #999;
}
.guarantee-btn{
    display: inline-block;
    min-width: 7.5rem;
    text-align: center;
    color: #fff;
    letter-spacing: 1px;
    font-size: 0.7rem;
    border-radius: 0px;
    margin-top: 1rem;
    transition: .5s;
    border: 1px solid #00aab0;
    font-family: '微软雅黑','Open Sans', sans-serif;
    font-weight: 700;
    transition: all 0.6s;
    border-radius: 0.2rem;
    background: #1bb38c;
    padding: 1rem 2rem;
    line-height:1.3333333;
}
.slick-dots {
    display: block;
    width: 100%;
    position: absolute;
    bottom:0;
    padding: 0;
    margin: 0;
    list-style: none;
    text-align: center;
}
.slick-dots li {
    position: relative;
    display: inline-block;
    width: 2rem;
    height: 2rem;
    margin: 0 5px;
    padding: 0;
    cursor: pointer;
}
.slick-dots .slick-active button:before{
    outline: none;
    color: #000;
    opacity: .75;
}
.slick-dots li button {
    font-size: 0;
    line-height: 0;
    display: block;
    width: 1rem;
    height: 1rem;
    padding: 0.4rem;
    cursor: pointer;
    color: transparent;
    border: 0;
    outline: none;
    background: transparent;
    transition: 0.2s ease-in-out;
    cursor: pointer;
}
.slick-dots li button:before{
    font-family: 'slick';
    font-size: 2rem;
    line-height: 1rem;
    position: absolute;
    top: 0;
    left: 0;
    width: 1rem;
    height: 1rem;
    content: '•';
    text-align: center;
    opacity: .25;
    color: #000;
    -webkit-font-smoothing: antialiased;
    box-sizing: border-box;
}
.guarantee-right{
    width:50%;
    height:auto;
    float: right;
}
#prev, #next{
    position: relative;
    top:-15rem;
    z-index: 3;
    cursor: pointer;
}
#prev{
    float: left;
    margin-left:0rem;
}
#next{
    float: right;
    margin-right:0rem;
}
#prev:hover, #next:hover{opacity: 0.5;filter:alpha(opacity=50);}
