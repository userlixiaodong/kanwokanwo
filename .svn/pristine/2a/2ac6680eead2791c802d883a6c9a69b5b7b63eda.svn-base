@charset "utf-8";
* {
    margin: 0;
    padding: 0;
    font-family: 微软雅黑;
}

html {
    font-size: 20px;
}
li{
    list-style-type: none;
}

@media screen and (max-width: 1920px) {
    html {
        font-size: 18px !important;
    }
}

@media screen and (max-width: 1300px) {
    html {
        font-size: 16px !important;
    }
}

@media screen and (max-width: 1280px) {
    html {
        font-size: 14px !important;
    }
}

@media screen and (max-width: 750px) {
    html {
        font-size: 12px !important;
    }

    body {
        max-width: 750px;
    }
}
/* iphone6 plus */
@media (min-device-width: 414px) and (max-device-width: 736px) and (-webkit-min-device-pixel-ratio: 3) {
    html {
        font-size: 25.875px;
    }
}
/* iphone 6 */
@media (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    html {
        font-size: 23.4375px;
    }
}
/* 判断iphone5 *//* 横屏竖屏判断方法与ipad一样 */
@media only screen and (min-device-width: 320px) and (max-device-width: 568px) {
    html {
        font-size: 20px;
    }
}

.clear {
    clear: both;
}
/*标题*/
.titles{
    width:100%;
    height:2rem;
    text-align: center;
    line-height:2rem;
    font-size:1.8rem;
    font-weight:400;
    color: #333;
    letter-spacing: 1px;
}
/*banner图start*/
.gis_banner {
    height: 25rem;
    margin-top: 3.7rem;
}
.ban-pic{
    width:100%;
    height:100%;
}
.ban-wx{
    position: absolute;
    top:5rem;
    right:8rem;
}
.gis_banner_btn {
    height: 3rem;
    position: absolute;
    text-align: center;
}

.signUp {
    display: inline-block;
    width: 8rem;
    height: 2.4rem;
    margin-top:1rem;
    background: #0cbc9d;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 2.4rem;
    color: #fff;
    font-family: "微软雅黑 Light";
    font-weight:bold;
    font-size:0.9rem;
    letter-spacing: 0.1em;
}

.signUp:nth-of-type(1) {
    /*float: left;*/
    /*margin-left: 6rem;*/
}

.signUp:nth-of-type(2) {
    float: right;
    margin-right: 6rem;
}
@media (max-device-width: 769px){
    .gis_banner {
        height: 15rem;
        margin-top: 5rem;
    }
    .ban-pic{
        width:100%;
        height:100%;
    }
    .ban-wx{
        width:6rem;
        height:auto;
        position: absolute;
        top:3rem;
        right:5rem;
    }
    .gis_banner_btn {
        height: 3rem;
        position: absolute;
    }

    .signUp {
        display: inline-block;
        width: 6.4rem;
        height: 2.4rem;
        margin-top:-0.5rem;
        background: #018b9b;
        border-radius: 0.2rem;
        text-align: center;
        line-height: 2.4rem;
        color: #fff;
        font-family: "微软雅黑 Light";
        font-weight:bold;
        font-size:0.8rem;
        letter-spacing: 0;
    }

    .signUp:nth-of-type(1) {
        /*float: left;*/
        margin-top: 0.5rem;
    }

    .signUp:nth-of-type(2) {
        float: right;
        margin-right: 3rem;
    }
    @media (max-device-width: 640px) {
        .gis_banner {
            height: 10rem;
            margin-top: 6rem;
        }
        .ban-wx{
            width:6rem;
            height:auto;
            position: absolute;
            top:1rem;
            right:1.2rem;
        }
    }
}
/*学院优势*/
.advantage{
    width:100%;
    height:auto;
    position: relative;
    margin:3rem auto;
}
.advantage ul{
    width:60%;
    height:27rem;
    margin:2rem auto;
}
.advantage ul li{
    list-style-type: none;
    width:30%;
    height:12rem;
    float: left;
    margin:1rem 0 0 1rem;
    text-align: center;
    overflow: hidden;
}
.advantage ul li h4{
    font-family:"微软雅黑 Light";
    color: #333;
    margin-top:6rem;
}
.advantage ul li span{
    display: block;
    width:80%;
    height:4rem;
    margin:1rem auto;
    font-size:0.8rem;
    font-family:"微软雅黑 Light";
    color: #7d7e7e;
    line-height:1.2rem;
}

@media (max-width: 769px) {
    .advantage ul{
        width:100%;
        height:46rem;
        margin:2rem auto;
    }
    .advantage ul li{
        list-style-type: none;
        width:45%;
        height:14rem;
        float: left;
        margin:1rem 0 0 1rem;
        text-align: center;
        overflow: hidden;
    }
    .advantage ul li h4{
        font-size:0.8rem;
        font-family:"微软雅黑 Light";
        color: #333;
        margin-top:7rem;
    }
    .advantage ul li span{
        display: block;
        width:80%;
        height:4rem;
        margin:1rem auto;
        font-size:0.7rem;
        font-family:"微软雅黑 Light";
        color: #7d7e7e;
        line-height:1.2rem;
    }

}
