    color: #aaaaaa;
    position: absolute;
    right: 0.5rem;
    top: 0.5rem;
}

.login-before p {
    font-family: "Microsoft Yahei";
    font-size: 0.9rem;
    margin-top: 4.5rem;
    width: 100%;
    text-align: center;
}

.login-before .login-btn {
    display: block;
    width: 7rem;
    height: 2.75rem;
    font-size: 1.2rem;
    line-height: 2.75rem;
