@charset "utf-8";
* {
    margin: 0;
    padding: 0;
    font-family: 微软雅黑;
}

html {
    font-size: 20px;
}

@media screen and (max-width: 1920px) {
    html {
        font-size: 18px !important;
    }
}

@media screen and (max-width: 1300px) {
    html {
        font-size: 16px !important;
    }
}

@media screen and (max-width: 1280px) {
    html {
        font-size: 14px !important;
    }
}

@media screen and (max-width: 750px) {
    html {
        font-size: 12px !important;
    }

    body {
        max-width: 750px;
    }
}

/* iphone6 plus */
@media (min-device-width: 414px) and (max-device-width: 736px) and (-webkit-min-device-pixel-ratio: 3) {
    html {
        font-size: 25.875px;
    }
}

/* iphone 6 */
@media (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    html {
        font-size: 23.4375px;
    }
}

/* 判断iphone5 *//* 横屏竖屏判断方法与ipad一样 */
@media only screen and (min-device-width: 320px) and (max-device-width: 568px) {
    html {
        font-size: 20px;
    }
}
.clear {
    clear: both;
}
/*名师阵容*/
.teachers{
    width:100%;
    height:auto;
    background: #f1f6f8;
    padding:3rem 0;
}
.teachers-wrapper{
    height:auto;
    margin:4rem auto 0;
    overflow: hidden;
}
.teachers ul{
    height:auto;
    margin-left:0;
}
.teachers ul li{
    height:28rem;
    text-align: center;
    float: left;
}
.en-name{
    color: #005f6a;
    font-style: italic;
    font-weight: bold;
    font-family:"Arial";
    font-size:0.8rem;
    margin-top:14rem;
}
.ch-name{
    color: #018b9b;
    font-family:"Adobe 黑体 Std";
    font-size:0.7rem;
}
.teacher-describe{
    display: block;
    width:80%;
    height:auto;
    margin:0 auto;
    font-family:"微软雅黑";
    color: #333;
    font-size:0.8rem;
    line-height:1.4rem;
}
#prev, #next{
    position: relative;
    top:-18rem;
    z-index: 3;
    cursor: pointer;}
#prev{
    float: left;
    margin-left:15rem;
}
#next{
    float: right;
    margin-right:15rem;
}
#prev:hover, #next:hover{opacity: 0.5;filter:alpha(opacity=50);}

@media (max-width: 769px) {
    .teachers{
        width:100%;
        height:auto;
        background: #f1f6f8;
        padding:3rem 0;
    }
    .teachers-wrapper{
        width:90%;
        height:auto;
        margin:1rem auto 0;
    }
    .teachers ul{
        width:100%;
        height:auto;
        margin-left:0;
        text-align: center;
    }
    .teachers ul li{
        display: inline-block;
        width:50%;
        height:auto;
        text-align: center;
        margin:1rem auto;
    }
    .en-name{
        color: #005f6a;
        font-style: italic;
        font-weight: bold;
        font-family:"Arial";
        font-size:1rem;
        margin-top:16rem;
    }
    .ch-name{
        color: #018b9b;
        font-family:"Adobe 黑体 Std";
        font-size:0.9rem;
    }
    .teacher-describe{
        width:80%;
        font-size:1rem;
        line-height:1.6rem;
    }
    #prev, #next{
        display: none;
    }
    @media (max-width:640px) {
        .teachers ul li{
            display: inline-block;
            width:100%;
            height:auto;
            text-align: center;
            margin:0 auto;
        }
        .en-name{
            color: #005f6a;
            font-style: italic;
            font-weight: bold;
            font-family:"Arial";
            font-size:0.8rem;
            margin-top:19rem;
        }
        .teacher-describe{
            display: block;
            width:94%;
            height:auto;
            margin:0 auto;
            font-family:"微软雅黑";
            color: #333;
            font-size:1rem;
            line-height:1.6rem;
        }
    }
}

