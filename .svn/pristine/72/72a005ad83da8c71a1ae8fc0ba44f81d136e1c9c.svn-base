.case{
    width:100%;
    height:auto;
    background: #f1f6f8;
    margin:2rem auto;
    padding:4rem 0 6rem;
    overflow: hidden;
}
.caseImg{
    width:50%;
    height:auto;
    margin:3rem auto;
}
.imgWrapper{
    width:100%;
    height:auto;
}
.imgWrapper li{
    list-style-type:none;
    margin-top:0.4rem;
    overflow: hidden;
}
.imgFirst{
    width:32.3%;
    height:19rem;
    float: left;
}
.imgWrapper img{
    width:100%;
    height:100%;
}
.img-hover{
    transition-duration:0.5s; /*过度的时间为0，5秒*/
    transform: scale(1.2);
    -webkit-transform: scale(1.2); /*Safari 和 Chrome*/
    -moz-transform: scale(1.2); /*Firefox*/
    -ms-transform: scale(1.2); /*IE9*/
    -o-transform: scale(1.2); /*Opera*/
}
.case-span1{
    height:19rem;
    position: relative;
    top:-19rem;
    left:0;
    line-height:19rem;
}
.imgInner{
    width:67.7%;
    height:auto;
    float: right;
}
.imgInner li{
    width:48%;
    height:9.3rem;
