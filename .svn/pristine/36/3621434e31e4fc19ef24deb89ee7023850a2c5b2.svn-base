/*==============================lx模块start================================*/
/*标题*/
.titles{
    width:100%;
    height:2rem;
    text-align: center;
    line-height:2rem;
    font-size:1.6rem;
    font-weight:400;
    font-family:"微软雅黑";
    color: #333;
    letter-spacing: 1px;
}
