/*05独家美国高校采访视频start*/
.interview-resume{
    width:100%;
    height:auto;
    background: url("/images/index-interview-bg.jpg") no-repeat;
    background-size:cover ;
    margin:0rem auto 0;
    padding:4rem 0 2rem;
}
.interview-content{
    width:60%;
    height:auto;
    margin:2rem auto;
}
.interview-content-top{
    width:90%;
    height:auto;
    margin:0 auto;
    overflow: hidden;
}
.interview-content-inner{
    width:45%;
    height:26rem;
    padding:5rem 2rem 1rem;
    position: relative;
}
.interview-content-inner:nth-of-type(1){
    background: url("/images/index-interview-leftBg.png") no-repeat center top;
    background-size:cover ;
    float: left;
    margin-left:2rem;
}
.interview-content-inner:nth-of-type(2){
    background: url("/images/index-interview-rightBg.png") no-repeat center top;
    background-size:cover ;
    float: right;
    margin-right:2rem;
}
.interview-inner-title{
    width:100%;
    height:3rem;
    margin:1rem auto 0;
}
.interview-inner-title li{
    list-style-type: square;
    color: #08b08f;
}
.interview-inner-title li span{
    display: block;
    font-size:0.9rem;
    color: #333;
    line-height:1rem;
    padding-left:0;
}
.interview-inner-enSpan{
    display: block;
    width:100%;
    height:auto;
    padding-left:0;
    margin:0 auto;
    font-size:0.8rem;
    font-family:"Arial";
    font-style: italic;
    color: #999;
}
.interview-inner-chSpan{
    display: block;
    width:100%;
    height:auto;
    padding-left:0;
    margin:0.5rem auto;
    font-size:0.7rem;
    line-height:1.4rem;
    font-family:"微软雅黑";
    color: #333;
}
.interview-describe{
    width:86%;
    height:2rem;
    background: url("/images/index-interview-dot.png") no-repeat top right;
    background-size:10%;
    text-align: left;
    font-size: 0.8rem;
    font-family: "微软雅黑";
    border-bottom: 1px solid #fff;
    padding:0;
    position: absolute;
    top: 23rem;
    margin: 0 auto;
}
.interview-describe span:nth-of-type(1){
    color: #01896e;
    font-family:"Arial";
    font-size:0.8rem;
    margin-right:1rem;
}
.interview-describe span:nth-of-type(2){
    color: #333;
    font-family:"微软雅黑";
    font-size:0.7rem;
    border-left:1px solid #999;
    padding-left:1rem;
}
.interview-content-bottom{
    list-style-type: none;
    width:100%;
    height:4rem;
    margin:1rem auto 0;
    text-align: center;
}
.interview-content-bottom li{
    width:33.3333333333%;
    height:4rem;
    text-align: left;
    padding-left:4rem;
    float: left;
}
.interview-content-bottom li span{
    display: block;
    width:100%;
    height:100%;
    font-size:0.9rem;
    font-family:"微软雅黑";
    color: #fff;
    line-height:4rem;
}
.interview-content-bottom li:nth-of-type(1){
    background: url(/images/index-interview-01.png) no-repeat left center;
}
.interview-content-bottom li:nth-of-type(2){
    background: url(/images/index-interview-02.png) no-repeat left center;
}
.interview-content-bottom li:nth-of-type(3){
    background: url(/images/index-interview-03.png) no-repeat left center;
}
.moreMessage{
    display: block;
    width:7rem;
    height:2rem;
    background-color:#08b08f;
    border-radius:0.1rem;
    margin:3rem auto 0;
    text-align: center;
    line-height:2rem;
    color: #fff;
    font-size:0.8rem;
}
.moreMessage:hover{
    background-color: #e1eeeb;
    color: #333;
}
@media (max-width: 1592px) {
    .interview-content{
        width:80%;
    }
}
@media (max-width: 1114px) {
    .interview-content{
        width:90%;
    }
}
@media (max-width: 986px) {
    .interview-content-inner{
        height:30rem;
    }
    .interview-describe{
        top:27rem;
    }
}
@media  (max-width: 800px){
    .interview-content{
        width:96%;
    }
}
@media (max-width: 878px) {
    .interview-content-bottom li{
        font-size:0.8rem;
    }
    .interview-content-bottom li span{
        line-height:normal;
        padding-top:0.6rem;
    }
}
@media (max-width: 769px) {
    .interview-content-bottom li{
        font-size:0.7rem;
    }
}
@media (max-width: 640px) {
    .interview-resume{
        margin:4rem auto 0;
        padding:2rem 0 1rem;
    }
    .interview-content{
        width:100%;
    }
    .separator{
        width:5rem;
    }
    .interview-content-top{
        width:100%;
        margin:0 auto;
        padding:0;
    }
    .interview-inner-title li{
        margin-left:1.6rem;
    }
    .interview-inner-title li span{
        font-size:1.2rem;
    }
    .interview-content-inner{
        width:90%;
        height:32rem;
        padding:6.5rem 1.5rem 1rem;
    }
    .interview-content-inner:nth-of-type(1){
        float: none;
        margin:0 auto;
    }
    .interview-content-inner:nth-of-type(2){
        float: none;
        margin:0 auto;
    }
    .interview-inner-enSpan{
        margin-top:1rem;
        font-size:1rem;
        line-height:1.4rem;
    }
    .interview-inner-chSpan{
        font-size:1rem;
        line-height:1.6rem;
    }
    .interview-describe {
        top: 28rem;
    }
    .interview-content-bottom{
        width:96%;
        margin-top:2rem;
    }
    .interview-content-bottom li{
        width:33.3333333333%;
        height:8rem;
        text-align: center;
        padding:3.5rem 0;
    }
    .interview-content-bottom li span{
        display: block;
        width:100%;
        height:100%;
        font-size:0.9rem;
        line-height:normal;
    }
    .interview-content-bottom li:nth-of-type(1){
        background: url(/images/index-interview-01.png) no-repeat center top;
    }
    .interview-content-bottom li:nth-of-type(2){
        background: url(/images/index-interview-02.png) no-repeat center top;
    }
    .interview-content-bottom li:nth-of-type(3){
        background: url(/images/index-interview-03.png) no-repeat center top;
    }
    .moreMessage{
        margin:5rem auto 0;
    }
}
/*04独家美国高校采访视频end*/
/*05独家美国高校采访视频start*/
.interview-video{
    width:100%;
    height:auto;
    margin:4rem auto 0;
    padding:2rem 0;
    background: #fff;
}
.interview-video ul{
    width:60%;
    height:auto;
    margin:2rem auto 0;
    overflow: hidden;
    position: relative;
}
.interview-video ul li{
    float: left;
    width:32%;
    height:13rem;
    margin:1rem 0.4rem;
}
.interview-video ul li img{
    width:100%;
    height:100%;
}
.video-introduce{
    width:100%;
    height:13rem;
    background: rgba(0,0,0,0.4);
    position: relative;
    top:-13rem;
    left:0;
    color: #fff;
    text-align: left;
    padding:1rem 0.5rem;
    /*display: none;*/
}
.borderLine{
    width:100%;
    height:100%;
    border:1px solid #fff;
}
.video-introduce p{
    font-weight: bold;
    font-size:1rem;
    text-align: center;
    margin:1rem auto 0;
    font-family:"微软雅黑","Arial";
}
.video-introduce span{
    display: block;
    width:90%;
    margin:1rem auto;
    text-align: center;
    line-height:1.6rem;
    font-size:0.8rem;
    font-family:"微软雅黑";
}
.videos-more{
    display: block;
    width:6rem;
    height:1.6rem;
    margin:0 auto;
    border:1px solid #fff;
    line-height:1.6rem;
    text-align: center;
    color: #fff;
    font-size:0.64rem;
    font-family:"微软雅黑";
}
.moreVideo{
    display: block;
    width:7rem;
    height:2rem;
    border:0.08rem solid #08b08f;
    border-radius:0.1rem;
    margin:3rem auto 0;
    text-align: center;
    line-height:2rem;
    color: #08b08f;
    font-size:0.8rem;
}
.moreVideo:hover{
    background-color: #08b08f;
    color: #fff;
}
@media (max-width: 1295px) {
    .videos-more{
        width:8rem;
    }
}
@media (max-width: 1820px) {
    .interview-video ul{
        width:70%;
    }
    @media (max-width: 1560px) {
        .interview-video ul{
            width:80%;
        }
        @media (max-width: 1375px) {
            .interview-video ul{
                width:90%;
            }
            @media (max-width: 955px) {
                .interview-video ul{
                    width:96%;
                }
                .interview-video ul li{
                    width:32%;
                    margin:1rem 0.2rem;
                }
            }
        }
    }
}
@media (max-width: 640px) {
    .interview-video ul{
        width:80%;
    }
    .interview-video ul li{
        width:100%;
    }
    .videos-more{
        width:8rem;
        height:1.8rem;
        line-height:1.8rem;
        font-size:0.9rem;
    }
    .moreVideo{
        margin:1rem auto;
        font-size:1rem;
    }
    .video-introduce p{
        font-size:1.2rem;
    }
    .video-introduce span{
        font-size:1rem;
    }
    @media (max-width: 400px) {
        .interview-video ul{
            width:96%;
        }
    }
}
/*==============================lx模块end================================*/




/*=============================顶部轮播图====================================*/
.indexBanner{
    margin: 0 auto;
    width: 100%;
    height: 520px;
    position: relative;
    overflow: hidden;
    margin-top: 3.6rem;
}

.indexBanner ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
    position: absolute;
    top: 0;
    left: 0;
    width: 99999px;
    padding-left: 0;
}

.indexBanner li {
    float: left;
    /*width:100%;*/
    padding-left: 0;
}

.indexBanner li img {
    width: 100%;
    padding-left: 0;
}

.bannerprev {
    position: absolute;
    left: 8%;
    top: 34%;
}

.bannernext {
    position: absolute;
    right: 8%;
    top: 34%;
}

.step {
    position: absolute;
    /*width: 10%;*/
    margin: 0 auto;
    left: 46.6%;
    bottom: 1rem;
}

.step span {
    display: block;
    width: 15px;
    height:15px;
    float: left;
    background-color: #8ddacb;
    border-radius: 100%;
    margin: 0 5px;
    cursor: pointer;
    padding-left: 0;
}

.step span.active {
    background-color: #08b08f;
}
.bannerprev:hover,.bannernext:hover{
    background-color: rgba(0,0,0,0.18);
}

