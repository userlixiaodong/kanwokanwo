.partersList li{
    width: 12.5%;
    height: 4rem;
    float: left;
    margin: 3rem 0;
}
.partersList li p{
    width: 88%;
    margin: 0 auto;
    padding: 0.5rem;
    border: 0.07rem solid #d2d2d2;
}
.partersList li p:hover{
    border: 0.1rem solid #72c954;
}
.businessParters .indexTitle{
    padding: 1rem 0 0rem 0;
}
.partersList li:nth-child(9){
    margin-left: 12.5%;
}
/*========================免费咨询样式============================*/
.mfzxBa{
    background: url("/images/newindex/mfzxBA.png") no-repeat center;
}
.mfzx{
    width: 63.8%;
    margin: 0 auto;
}
.mfzxLeft,.mfzxRight{
    width: 50%;
    float: left;
}
.mfzxLeft .indexTitle,.mfzxRight .indexTitle{
    color: #fff;
    font-size: 1.4rem;
    font-family: "Microsoft Yahei";
    padding:0;
    text-align: left;
}
.mfzxRight input,.mfzxRight textarea{
    width: 80%;
}
.mfzxLeft{
    padding: 4rem 6rem 4rem 0rem;
}
.mfzxRight{
    padding: 4rem 0rem 0rem 7rem;
}
.mfzxLeft h6{
    color: #fff;
    font-size: 0.9rem;
    font-family: "Microsoft Yahei";
    margin: 2.4rem 0;
    font-weight: normal;
    line-height: 1.8rem;
}
.mfzxLeft p span{
    display: block;
    font-size: 0.82rem;
    color: #fff;
    margin: 1rem 0;
}
.mfzxLeft p span:before{
    display: block;
    content: "";
    width: 3rem;
    height: 2rem;
    background: url(/images/newindex/mfzxIcon.png) no-repeat center;
    float:left;
    margin-top: -0.4rem;
}
.mfzxRight p i{
    display: block;
    float: left;
    height: 2.2rem;
    line-height: 2.2rem;
    color: #fff;
    font-size: 0.9rem;
    width: 20%;
}
.mfzxRight p:nth-child(1){
    margin: 1.8rem 0 1rem 0;
}
.mfzxRight p input{
    height: 2.2rem;
}
.mfzxRight p textarea{
    overflow-y: scroll;
}
.mfzxRight button{
    width: 80%;
    float: right;
    height: 2rem;
    color: #fff;
    font-size:1rem ;
    border: none;
    background-color:#72c954;
    display:block;
}
.mfzxRight button:hover{
    background-color: #19c09f;
}
.mfzxRight span{
    float: right;
    width: 80%;
    color: #fff;
    display: block;
    text-align: center;
    padding: 0.8rem 0;
    font-size: 0.88rem;
}

/*底部样式*/
.newBottom{
    background-color: #333;
    padding: 2rem 0;
    margin-top: -1rem;
}
.bottomBox{
    width: 64%;
    margin: 0 auto;
    color: #fff;
}
.bottomBox .icp{
    color: #fff;
    text-align: center;
    font-size: 0.9rem;
    font-family: "Microsoft Yahei";
}
.bottomBox .img{
    margin-left: 2rem;
}
.bottomBox .navList li{
    width: 33.33%;
    float: left;
}
.navList li:nth-child(1),.navList li:nth-child(2){
    margin: 2rem 0 3rem 0;
}
.navList li:nth-child(2){
    border-left: 0.08rem solid #fff;
    padding-left: 5rem;
}

.navList li:nth-child(1) a{
    color: #fff;
    text-decoration: underline;
    display: block;
    width: 50%;
    float: left;
    font-family: "Microsoft Yahei";
    font-size: 0.84rem;
    height: 2rem;
}
.navList li:nth-child(2) a{
    color: #fff;
    text-decoration: underline;
    display: block;
    font-family: "Microsoft Yahei";
    font-size: 0.84rem;
    height: 2rem;
}
.navList li:nth-child(3) span{
    font-size: 0.9rem;
    font-family: "Microsoft Yahei";
}
.navList li:nth-child(3) span:before{
    display: inline-block;
    content: "";
    width: 3rem;
    height: 3rem;
    background: url(/images/map_icon.png) no-repeat 0rem -10rem;
    background-size: 2.4rem;
}
.navList .kanwoewm img {
    width:30%;
    margin: 1rem 1rem 0 0;
}


/*媒体查询样式*/
@media (max-width: 1820px){
    /*==============================================服务推荐导航============================*/
    .fwtjList{
        width: 80%;
    }
    .fwtjList li a{
        width: 80%;
    }
    /*=======================================背景提升================================*/
    .bjtsList li{
        width: 23%;
        height: 21rem;
    }

    /* 成功案例*/
    .cgal ul{
        width: 80%;
    }
   /* 免费咨询*/
    .mfzx{
        width: 80%;
    }
}
@media (max-width: 1350px){
    .fwtjList li i{
        background-size: 97%;
    }
    .fwtjList{
        width:100%;
    }

}
@media (max-width: 1024px){
    /*==================================顶部轮播图========================================*/
    .step{
        left: 42%;
    }
    /*=================================服务推荐导航======================================*/
    .fwtjList li a{
        width: 63%;
    }
    /*=======================================背景提升================================*/
    .bjtsList{
        width: 90%;
    }
    /*院校库信息*/
    .yxkInfo{
        height: 34rem;
    }
    /* 成功案例*/
    .cgal ul{
        width: 94%;
    }
    .cgal ul li{
        width: 24%;
    }

    /*合作伙伴*/
    .partersList li{
        width:14.28%;
        margin: 0 0 5rem 0;
    }
    .partersList li:nth-child(9){
        margin-left: 0;
    }
    .businessParters ul{
        width: 90%;
        margin: 3rem auto;
    }
    .partersList li p{
        width: 92%;
    }
    /*免费咨询*/
    .mfzxLeft{
        padding: 4rem 4rem 4rem 0rem;
    }
    .mfzxRight{
        padding: 4rem 0rem 4rem 4rem;
    }
    /*底部*/
    .bottomBox{
        width: 86%;
    }

}
@media (max-width: 980px){
    /*=========院校库信息===========*/
    .yxkInfo{
        width: 60rem;
    }
    .yxkInfo li{
        width: 60rem;
    }
    /*免费咨询*/
    .mfzxRight,.mfzxLeft{
        float: none;
        width: 100%;
    }
    .mfzxLeft{
        padding: 0;
        margin-bottom: 3rem;
    }
    .mfzxRight{
        padding:0;
    }
    .mfzxBa {
        background: none;
        background-color: #3e908a;
    }
    .mfzxLeft .indexTitle, .mfzxRight .indexTitle {
        padding: 0 0 1rem;
        text-align: center;
    }
    .mfzxLeft h6{
        margin: 0;
    }
    .mfzxBa{
        padding: 2rem 0;
    }
    .indexTitle i {
        border-bottom: 0.1rem solid #fff;
    }
}
@media (max-width: 768px){
    /*=================================服务推荐导航======================================*/
    .fwtjList li a{
        width: 81%;
    }
    /*=========院校库信息===========*/
    .yxkRight,.yxkLeft{
        float: none;
        width: 104%;
        padding: 1rem 0  1rem 1rem;

    }
    .pageNum{
        display: none;
    }
    .yxkInfo {
        width: 87%;
        /* overflow: inherit;*/
        height: auto;
    }
    .yxkLeft .bigImg,.yxkprev,.yxknext{
        display: none;
    }
    .yxkInfo ul{
        width: auto;
        position: static;
    }
    .yxkInfo ul li{
        float: none;
        border-bottom: 0.1rem solid #9fd4c8;
        padding-bottom: 1rem;
        width: auto;
        height: 38rem;
    }
    .yxkRight .jbxx table{
        width: 94%;
    }
    .yxkRight .literature {
        width: 93%;
    }
    .schoolName{
        float: left;

    }
    .grade{
        float: right;
    }
    .yxkRight .grade span{
        width: 34%;
    }
    .yxkRight .schoolName span:after{
        width: 7rem;
        height: 4rem;
    }
    .jbxx p{
        margin-top: 6rem;
        text-align: left;
    }
    .colleges-content h3{
        font-size: 1.3rem;
        margin-top: -4rem;
    }
    .colleges-column{
        margin-top: -13rem;
    }

    .colleges-btn {
        font-size: 30px;
        margin-top: 8rem;
    }
    .partersList li{
        width:25%;
        margin: 0 0 9rem 0;
    }
    /*底部*/
    .navList li:nth-child(1){
        width: 100%;
    }
    .navList li:nth-child(1) a{
        width: 25%;
    }
    .navList li:nth-child(2){
        width: 36%;
    }
   .navList li:nth-child(3){
       width: 64%;
   }

    .navList .kanwoewm img{
        width: 24%;
    }
    .navList li:nth-child(1), .navList li:nth-child(2) {
        margin: 0.8rem 0 0 0;
    }
    .navList li:nth-child(2) {
        border-left: none;
        padding: 0rem 2rem 0 0;
        margin-top: 4rem;
        text-align: right;
    }
}
@media (max-width: 767px){
    /*=================================服务推荐导航======================================*/
    .fwtjList li a{
        width: 100%;
        text-align: center;
    }
    .fwtjList li i{
        display: block;
        float: none;
        margin:0 auto;
    }
    .fwtjList li span{
        float: none;
    }
    .fwtjList{
        height: 6rem;
        padding: 0 0.5rem;
    }
    .fwtjList li span{
        font-size: 0.7rem;
        padding-left: 0;
    }
    .fwtjList li{
        /* border: none;*/
        padding-left: 0;
    }
    .fwtjList li span br{
        display: none;
    }

    /*=======================================背景提升================================*/
    .bjtsList li{
        width: 46%;
        margin: 0.8rem;
        height: 25rem;
    }

    .bjtsList{
        height: 56rem;
    }

    /*=========院校库信息===========*/
    .yxkLeft .smallImg img {
        width: 46.5%;
        margin-bottom: 1rem;
    }
    .yxkInfo ul li{
        width: 46rem;
        height: 62rem;
    }
    /* 成功案例*/
    .cgal ul{
        width: 96%;
    }
    .cgal ul li{
        width: 48%;
    }
    .cgal{
        height: 100rem;
    }


}
@media (max-width: 560px){
    /*=======================================背景提升================================*/
    .bjtsList li{
        float: none;
        width: 80%;
        margin: 1.5rem auto;
        height: auto;
    }
    .bjtsList li:nth-child(4){
        margin-right: auto;
    }
    .bjtsList{
        height: auto;
    }
    .indexTitle {
        padding: 1rem 0;
        font-size: 1.2rem;
    }
    .bjts{
        padding: 1rem 0 2rem 0;
    }
    /*底部*/
    .navList li:nth-child(1) a {
        width: 33.33%;
        font-size: 0.6rem;
        margin-top: 1rem;
    }
    .navList .kanwoewm img {
        width: 36%;
    }
    .bottomBox {
        width: 92%;
    }
    .cgal{
        height: 82rem;
    }

}
@media (max-width: 414px){
    .indexBanner{
        margin-top: 6rem;
    }
    .bannerprev img,.bannernext img{
        width: 60%;
    }
    .step span{
        width: 10px;
        height: 10px;
    }
    .step{
        left: 39%;
    }
    .yxkInfo li {
        width: 37rem;
    }
    .yxkRight, .yxkLeft{
        width: 102%;
    }
    .xxkSchool .moreBtn {
        margin-top: 3rem;
    }
    /*=======================================背景提升================================*/
    .bjtsList li{
        width: 94%;
    }
    .yxkInfo ul li{
        width: auto;
    }
    /* 成功案例*/

    .cgal ul li a:after{
        background-size: 80%;
        right: -4rem;
    }
    .colleges-column {
        margin-top: 4rem;
    }
    .colleges-content h3 {
        margin-top: 0rem;
    }
    .colleges-btn {
        margin-top: 4rem;
    }
    /* 合作伙伴*/
    .businessParters ul{
        width: 76%;
    }
    .partersList li{
        width:50%;
        margin: 0 0 10.5rem 0;
    }
    /*免費咨詢*/
    .mfzx {
        width: 86%;
    }
    .mfzxRight p i{
        width: 100%;
    }
    .mfzxRight input, .mfzxRight textarea ,.mfzxRight button,.mfzxRight span{
        width: 100%;
    }
    .mfzxRight span{
        font-size: 0.8rem;
    }
    .cgal{
        height: 77rem;
    }
}
@media (max-width: 375px){
    .partersList li {
        margin: 0 0 8.5rem 0;
    }
    .cgal{
        height: 45rem;
    }
}
@media (max-width: 320px) {
    .step {
        left: 34%;
    }
    .partersList li {
        margin: 0 0 6.5rem 0;
    }
    .cgal{
        height: 40rem;
    }

}