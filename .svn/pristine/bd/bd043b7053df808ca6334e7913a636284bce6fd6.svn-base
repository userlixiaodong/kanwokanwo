   /* background: url(/images/CIS-video.jpg) no-repeat;*/
}
.videoWrapper{
    width: 44.3rem;
    height: 87%;
    margin: 2rem auto;
    background-color: rgb(0,0,0);
    cursor: pointer;
    padding: 1rem;
}
.videoSignUp{
    display: block;
    width:8rem;
    height:2.4rem;
    background: #0085a7;
    border-radius:0.2rem;
    margin:1rem auto;
    text-align: center;
    line-height:2.4rem;
    color: #fff;
    font-family:"微软雅黑 Light";
    font-size:1.2rem;
    font-weight:bold;
    letter-spacing: 0.1em;
}
.play{
    position: relative;
    top: -55%;
    left:50%;
    margin-left:-30px;
    display: inline-block;
    font-family: 'Glyphicons Halflings';
    font-style: normal;
    font-weight: normal;
    font-size: 60px;
    color: #fff;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
}
.play:before{
    content: "\e029";
    box-sizing: border-box;
}

