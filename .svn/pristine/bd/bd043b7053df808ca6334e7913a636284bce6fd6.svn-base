.clear {
    clear: both;
}

.parters{
    padding: 3rem 0;
    width: 80%;
    margin: 0 auto;

}
.parters li{
    width: 20%;
    float: left;
    height: 10rem;
}
.parters li p{
    width: 70%;
    height: 70%;
    margin: 0 auto;
    border: 1px solid #ccc;
    text-align: center;
    line-height: 6.7rem;
    padding: 0;
    background-color:#fff9f9;
}
.parters p span{
    font-size: 1rem;
    padding: 0;
}
.weixinFrame {
    width: 8rem;
    height: 14rem;
    background: url(/images/weixinFrame.png) no-repeat;
    background-size: contain;
    position: fixed;
    left: 0;
    top: 30%;
    z-index: 999;
}

.gis_banner {
    height: 25rem;
    background: url(/images/BA-manteng.jpg) no-repeat center top;
    background-size: 100% 25rem;
    margin-top: 3.7rem;
}
.gis_banner_btn {
    height: 3rem;
    position: absolute;
}

.signUp {
    display: inline-block;
    width: 8rem;
    height: 2.4rem;
    margin-top:1rem;
    background: #018b9b;
    border-radius: 0.2rem;
    text-align: center;
    line-height: 2.4rem;
    color: #fff;
    font-family: "微软雅黑 Light";
    font-weight:bold;
    font-size:0.9rem;
    letter-spacing: 0.1em;
}

.signUp:nth-of-type(1) {
    float: left;
    margin-left: 7.4rem;
}

.signUp:nth-of-type(2) {
    float: right;
    margin-right: 3rem;
}
@media (max-device-width: 320px){
    .signUp:nth-of-type(1){
        margin-top: 0.6rem;
    }
    .signUp:nth-of-type(2){
        margin-top: 0.6rem;
    }
    .gis_banner {
        height: 12rem;
        background: url(/images/BA-manteng.jpg) no-repeat -19rem top;
        background-size: 190% 12rem;
        margin-top: 6rem;
    }
    .application .consulting{
        margin: -2rem auto 0 auto;


    }
@media (max-device-width: 750px) {
    .application .consulting {
        margin: -2rem auto 0 auto;
    }
    }

    .gis_banner_btn {
        height: 3rem;
        position: absolute;
    }

    .signUp {
        display: inline-block;
        width: 6.4rem;
        height: 2.4rem;
        margin-top:-0.5rem;
        background: #018b9b;
        border-radius: 0.2rem;
        text-align: center;
        line-height: 2.4rem;
        color: #fff;
        font-family: "微软雅黑 Light";
        font-weight:bold;
        font-size:0.8rem;
        letter-spacing: 0;
    }

    .signUp:nth-of-type(1) {
        float: left;
        margin-left: 4rem;
    }

    .signUp:nth-of-type(2) {
        float: right;
        margin-right: 3rem;
    }
}
@media (max-device-width: 375px)and (min-device-width: 321px){
    .gis_banner {
        height: 13rem;
        background: url(/images/BA-manteng.jpg) no-repeat -17rem top;
        background-size: 170% 13rem;
        margin-top: 6rem;
    }
}
@media (max-device-width: 414px)and (min-device-width: 376px){
