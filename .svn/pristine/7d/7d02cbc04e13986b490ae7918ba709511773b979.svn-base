*{
    margin: 0;
    padding: 0;
}
body,html{
    width:100%;
}
section,.row{
    margin: 0px!important;
    padding: 0px!important;

}
.container-fluid-2{
    padding:0!important;
    background-color: #f0f0f0;
}
.appointment{
    width: 150px;
    height: 70px;
    position: fixed;
    z-index: 10000;
    background-color: #0cc394;
    -webkit-border-radius: 75px;
    -moz-border-radius: 75px;
    border-radius: 11px;
    right: 0;
    top: 50%;
    margin-top: -75px;
    box-shadow: 0 0 10px 0 #3d3d3d;
}
.appointment a{
    display: block;
    width:100%;
    height:100%;
    line-height: 70px;
    text-align: center;
    font-size: 25px;
    color: #fff;
    font-family: "Microsoft Yahei";
}
/*.appointment:hover{*/
    /*animation: change 2s infinite;*/
/*}*/
/*@keyframes change {*/
    /*100%{*/
        /*transform: rotate3d(0,1,0,360deg);*/
    /*}*/
/*}*/
.login-before{
    width: 700px;
    height: 340px;
    position: fixed;
    left: 50%;
    top: 50%;
    z-index:1000;
    text-align: center;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    background: #e1e4e8;
    display: none;
}
.consult-fail{
    width: 700px;
    height: 340px;
    position: fixed;
    left: 50%;
    top: 50%;
    z-index:1000;
    text-align: center;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    background: #e1e4e8;
    display: none;
}
.login-before .login-before-close-btn,.pay-close-btn,.consult-fail .consult-fail-close-btn{
    color: #aaaaaa;
    position: absolute;
    right: 10px;
    top: 10px;
}
.login-before p,.consult-fail p{
    font-family: "Microsoft Yahei";
    font-size: 18px;
    margin-top: 90px;
    width:100%;
    text-align: center;
}
.login-before .login-btn{
    display: block;
    width: 140px;
    height: 55px;
    font-size: 24px;
    line-height: 55px;
    color: #fff;
    text-align: center;
    margin: 70px auto;
    background-color: #28b38d;
}
.consult{
    display: none;
    padding: 30px;
    width: 800px;
    height: 800px;
    background: #e1e4e8;
    position: fixed;
    z-index:1000;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
}
.consult .consult-close-btn{
    color: #aaaaaa;
    position: absolute;
    right: 10px;
    top: 10px;
}
.byte-count{
    position: absolute;
    right: 30px;
}
.consult input,.consult select{
    width: 240px;
    height: 30px;
}
.consult .cost{
    margin-left: 5px;
    font-weight: bold;
}
.consult .consult-time{
    font-size: 16px;
    font-family: "Microsoft Yahei";
    margin-right: 30px;
}
/*.consult .consult-time .consult-time-name{*/
    /*display: block;*/
/*}*/
.consult .consult-time b{
    font-size: 16px;
    color: red;
    margin:0 2px;
}
.consult ul{
    font-family: "Microsoft Yahei";
    padding:0 50px;
    font-size: 14px;
    color: #e09400;
}
.name-input,.phone-input,.email-input{
    margin-top: 16px;
}
.consult ul li,.consult .tip,.tip-bottom ,.pay-tip{
    font-size: 14px;
    color: #e09400;
    font-family: "Microsoft Yahei";
}
.another{
    line-height: 60px;
}
.a-quesiton{
    width:100%;
    display: block;
    height: 150px;
    font-size: 14px;
    resize: none;
}
.tip-bottom{
    display: none;
    margin-top: 30px;
    width:100%;
    text-align: center;
}
.consult-btn{
    display: block;
    width: 170px;
    height: 55px !important;
    background-color: #28b38d;
    border:none;
    line-height: 55px;
    font-size: 24px;
    margin:0 auto;
    color: #fff;
}

.pay{
    display: none;
    padding: 50px;
    width: 800px;
    height: 740px;
    background: #e1e4e8;
    position: fixed;
    z-index:1000;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);

}
.pay h3{
    font-family: "Microsoft Yahei";
    text-align: center;
}
.pay h2{
    font-family: "Microsoft Yahei";
    font-size: 20px;
    color: #28b38d;
    text-align: center;
    margin:60px 0;
}
.pay h4{
    text-align: center;
    font-family: "Microsoft Yahei";
}
.pay h4 em{
    display: inline-block;
    width:40%;
    border-bottom: 1px solid #3d3d3d;
}
.pay ul{
    width:75%;
    height: 250px;
    padding: 20px;
    margin:0 auto;

}
.pay ul li{
    width: 200px;
    height: 220px;
    background-color: #0cc394;
    float: left;
    margin-left: 30px;
    position: relative;
    padding: 0;
}

.pay ul li img{
    width: 180px;
    height: 180px;
    display: block;
    margin: 10px auto;
}

.pay ul li p{
    position: absolute;
    bottom:-6px;
    width:100%;
    display: block;
    text-align: center;
    font-family: "Microsoft Yahei";
}
.pay .pay-tip{
    margin: 20px;
}
