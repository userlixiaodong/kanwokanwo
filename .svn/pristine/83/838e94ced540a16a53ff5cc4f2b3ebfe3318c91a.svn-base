.usf-contanier{margin-bottom: 40px !important;    margin-top: 75px;}
.usf-contanier .media-item-description {height: auto;  padding-bottom: 0;}
.usf-contanier,.usf-contanier strong,.usf-contanier h1,.usf-contanier h2{ font-family:Microsoft YaHei !important;}
.usf-contanier h1{ font-size:34px !important;}
.usf-contanier h2{ font-size:22px !important;}
@media (max-width: 768px){.usf-contanier h1,.usf-contanier h1{font-size: 22px;}}
.usf-contanier p{ font-size:16px !important;}
.banner-content{background:rgba(0,0,0,.5);color:#fff;position:absolute;left:0;width:100%;}
.banner-content a{color:#fff;}
.banner-top{height:50px;line-height:50px;top:0;padding-left:40px;}
.banner-bottom{bottom:0;height:88px;}
.banner-bottom dl{max-width:78%;margin:0 auto;}
.banner-bottom dd{margin-left:254px;max-width:100%;height:50px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;word-wrap:normal; margin-top: 18px;}
.banner-bottom dd strong{letter-spacing: -.5px;font-weight:bold;font-size:30px;}
@media (max-width: 1200px){.banner-bottom dl{max-width:90%;overflow:hidden;}
.banner-bottom dt{width:80px;height:80px;top:-18px !important;z-index:1;}
.banner-bottom dd strong{font-size:16px;}
.banner-bottom{height:50px !important;bottom:-50px;background:rgba(0,0,0,1);padding-top:4px;}
.banner-bottom dd{margin-left:90px;height:auto;}
.banner-like{line-height:50px !important;background:rgba(0,0,0,1);padding-top:10px;}
.banner-like i{font-size:14px;}}
@media screen and (min-width: 1200px) and (max-width: 1300px){.banner-bottom{height:80px !important;bottom:0 !important;}
.banner-bottom dd strong{font-size:26px;}
.banner-bottom dd{padding-top:6px;height:40px;}}
@media (max-width: 414px){.banner-like a{margin:0 4px !important;font-size:14px;}}
.banner-bottom dt{position:absolute;top: -50px;}
.banner-like{font-size:16px;line-height:88px;}
.banner-like a{margin:0 20px;}
.banner-like i{font-size:30px;margin-right:10px;top: 9px !important;}
.banner-like a.active i{color:#F00;}
.usf-btn{display:block;margin:10px auto;background:#eb6100;font-size:18px;font-style:oblique;color:#fff;width:146px;height:46px;line-height:46px;text-align:center;}
@media (max-width: 1200px){.usf-btn{ margin: 140px auto 0 !important;}}
.usf-left-model h2,.usf-left-model-bottom h2{width:70%;margin:0 auto !important;color:#fff;font-size:30px;padding:20px 0;background:#4d4d4d;margin:50px 0;text-transform:capitalize;letter-spacing:.6px;}
.usf-left-model ul,.usf-left-model dl{max-width:400px;margin:30px auto;}
@media (max-width: 414px){.usf-left-model ul,.usf-left-model dl,.usf-left-model-bottom ul{width:auto;padding:0 20px;}}
.usf-left-model ul li{margin:10px 0;}
.usf-left-model ul li a{color:#888888;font-size:16px;}
.usf-left-model ul li a i,.usf-left-model ul li a em{display:inline-block;}
.usf-left-model-top ul li a{color:#888888;font-size:16px;}
.usf-left-model-top ul li a i{vertical-align:top;top:6px !important;margin-right:10px;}
.usf-left-model-top ul li a em{width:90%;}
.usf-left-model-top ul li a strong{font-weight:bold;margin-right:10px;display:inline-block;vertical-align: top;margin-top: 7px;}
@font-face{
	font-family: "usficonfont";
	src: url('/statics/demo/fonts/jxsicon/iconfont.eot?t=1464329549');/* IE9*/
	src: url('/statics/demo/fonts/jxsicon/iconfont.eot?t=1464329549#iefix') format('embedded-opentype'),/* IE6-IE8*/
	url('/statics/demo/fonts/jxsicon/iconfont.woff?t=1464329549') format('woff'),/* chrome, firefox*/
	url('/statics/demo/fonts/jxsicon/iconfont.ttf?t=1464329549') format('truetype'),/* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
	url('/statics/demo/fonts/jxsicon/iconfont.svg?t=1464329549#iconfont') format('svg');/* iOS 4.1-*/}
.usficonfont{font-family:"usficonfont" !important;font-size:26px;font-style:normal;color:#366699 !important;}
.usf-icon-facebook:before{content: "\e600";}
.usf-icon-xiangji:before{content: "\e601";}
.usf-icon-twitter:before{content: "\e602";color:#48b0e3;}
.usf-icon-twitter{margin-top:2px;}
.usf-left-model-top .usf-btn{ margin-left: 348px;}
@media screen and (min-width: 1201px) and (max-width: 1430px){.usf-left-model-top .usf-btn{ margin-left: 286px;}}
.usf-left-model-center ul li a{width:90%;display:inline-block;font-size:14px;}
.usf-left-model-center ul li em{width:10px;height:10px;border-radius:50%;background:#4d4d4d;display:inline-block;vertical-align:top;margin:6px 10px 0 0;}
.usf-left-model-center ul li a strong{display:block;font-weight:bold;font-size:16px;}
.usf-left-model-center ul li a,.usf-left-model-center ul li a strong{font-family:Microsoft YaHei;}
.usf-left-model-center dt{height:40px;line-height:40px;}
.usf-left-model-bottom ul{margin:0 auto;width:70%;padding-top: 3%;}
.usf-left-model-bottom ul li{float:left;width: 33%;text-align: center;}
@media (max-width: 414px){.usf-left-model-bottom ul li{float:left;width:20%;margin-left:5%;margin-right:5%;}}
.usf-left-model-bottom ul li .more{color:#4d4d4d;margin:40px 0 0 20px;display:block;}
/* tab*/.usf-tab{position:relative;margin:40px auto;}
.usf-tab,.usf-tab .usf-tab-box{width:100%;}
.usf-tab .usf-tab-menu{width:100%;float:left;position:absolute;z-index:1;}
.usf-tab .usf-tab-menu .selected{background-color:#1bb38c;}
.usf-tab .usf-tab-menu li{font-size:30px;font-weight:bold;font-family:微软雅黑;color:#898989;background:#f9f9f9;float:left;width:50%;height:80px;line-height:80px;text-align:center;}
@media (max-width: 768px){.usf-tab .usf-tab-menu li{font-size:18px;}}
.usf-tab .usf-tab-box{top:80px;position:relative;}
.usf-tab .usf-tab-menu .selected{cursor:pointer;color:#fff;}
.hide{display:none;}
.usf-tab-box > div{max-width:970px;margin:0 auto;padding:20px;}
.usf-add-content{height:140px;}
.usf-add-content textarea{height:100%;}
.usf-add-content a{height:100%;display:block;text-align:center;color:#898989;padding-top:42px;border:1px solid #ccc;border-left:none;}
.usf-add-content a em{display:inline-block;width:100%;}
.usf-add-content a.usf-post{background:#1bb38c;color:#fff;padding:0;line-height:140px;border:none;}
.usf-add-content a.usf-post:hover{background:#08ac82;}
@media (max-width: 768px){.usf-add-content a{height:90px;padding-top:20px;border-top:none;border-left:1px solid #ccc;border-right:none;margin-bottom:20px;}
.usf-add-content a.usf-post{line-height:90px;}}
.usf-item{margin-top:20px;}
.usf-item p{color:#888888;font-size:18px;padding-right: 12%;}
.usf-item em{display:block;color:#cecdcd;}
.pop-up-box{margin-top:18px;display:block;}
.praise-btn{width:70%;color:#dcdcdc;margin:20px auto 0;}
.praise-btn a,.praise-btn span,.praise-btn em{display:inline-block;margin-left:10px;}
.praise-btn span{width:70%;border-top:1px solid #ccc;vertical-align: top;margin-top: 22px;}
.praise-btn a{color:#dcdcdc;font-size:30px;}
.praise-btn a.active{color:#C71B1B;}
.praise-btn em{margin-top: 12px;vertical-align: top;}
.pagination-sm > li > a, .pagination-sm > li > span{font-size:16px;}
@media (max-width: 463px){.praise-btn span{width:54%;}}