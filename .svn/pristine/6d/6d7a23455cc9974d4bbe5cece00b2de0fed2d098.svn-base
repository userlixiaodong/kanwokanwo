/* iphone6 plus */
@media (min-device-width: 414px) and (max-device-width: 736px) and (-webkit-min-device-pixel-ratio: 3) {
    html {
        font-size: 25.875px;
    }
}

/* iphone 6 */
@media (min-device-width: 375px) and (max-device-width: 667px) and (-webkit-min-device-pixel-ratio: 2) {
    html {
        font-size: 23.4375px;
    }
}

/* 判断iphone5 *//* 横屏竖屏判断方法与ipad一样 */
@media only screen and (min-device-width: 320px) and (max-device-width: 568px) {
    html {
        font-size: 20px;
    }
}
.clear {
    clear: both;
}
.container_box{
    margin: 68px 0 0 0;
}

.top_banner {
    width: 100%;
    height: 25rem;
    background: url(/images/int_banner2.jpg) no-repeat center;
}

.top_banner .int_text {
    width: 52%;
    height: 24rem;
    margin: 0 auto;
    text-align: center;
    padding: 2.6rem 2rem 0 0;
    color: white;
   /* background: url(/images/BA_line.png)no-repeat 0px 3.2rem;
    background-size: 100%*/;
}
.int_text div{
    border: 0.3rem solid #b5b01f;
    position: relative;
    padding-bottom: 4.4rem;
}

.top_banner .int_text h2 {
    padding-top: 2.1rem;
    font-size: 4.4rem;
    font-weight: normal;
    font-family: 微软雅黑;
   /* margin-left: 3.2rem;*/
}

.top_banner .int_text p {
    font-size: 1.6rem;
    letter-spacing: 0.3rem;
   /* padding-left: 2rem;*/
    font-family: 微软雅黑;
}

.top_banner .int_text span {
    position: absolute;
    display: block;
    margin-top: 2.7rem;
    font-size: 1.6rem;
    font-family: 微软雅黑;
   /* margin-left: 1.8rem;*/
    left: 30%;
    background-color: #b5b01f;
    padding: 0.5rem;

}

.cont {
    width: 100%;
    background-color: #f4f7fa;
    padding: 6rem 0;
}

.intSummary {
    width: 67rem;
    margin: 0 auto;
    height: 35rem;
    background-color: white;
    box-shadow: 0 0.5rem 2rem #997;
    padding: 2rem 2.2rem;
}

.intSummary_cont {
    margin-top: 1.2rem
}
.intSummary_cont img{
    width: 50%;
}

.intSummary_text {
    width: 43%;
    height: auto;
    float: right;
}

.intSummary_text h5 {
    font-size: 1.7rem;
    color: #333333;
    font-weight: normal;
    padding-bottom: 1.4rem;
    margin-top: 0.2rem;
    font-family: 微软雅黑;
}

.intSummary_text p {
    ffont-size: 0.9rem;
    color: #666666;
    line-height: 2rem;
    font-family: 微软雅黑;

}

.int_list {
    width: 67rem;
    margin: 0 auto;
    list-style: none;
}

.int_list li {
    float: left;
    list-style: none;
    width: 32.2%;
    height: 40rem;
    margin-top: 5rem;
    text-align: center;
    background-color: #f4f7fa;
    padding-left: 0;
    border-bottom: 0.23rem solid #20abbb;
    border-radius: 0.8rem;
}
.int_list li:nth-child(2){
    margin: 5rem 1rem 0 1rem;
}
.int_list li h6 {
    color: #1e919e;
    font-size: 1.3rem;
    font-weight: normal;
    margin-top: 1.8rem;
    font-family: 微软雅黑;
}

.int_list li p {
    width: 86%;
    margin: 0 auto;
    text-align: left;
    padding: 0.5rem 0;
    line-height: 1.6rem;
    font-size: 0.93rem;
    color: #666666;
    text-align: justify;
    font-family: 微软雅黑;
}
.int_list li:nth-child(3) p{
    padding: 0;
}
/*.int_list  li b{
    color: #0a0a0a;
}*/
.int_list li:nth-child(3) span{
    font-family: 微软雅黑;
    padding-left: 2.4rem;
    display: block;
    font-size: 0.97rem;
}

.map {
    width: 100%;
    height: auto;
    background: url(/images/int_map.png) no-repeat center;
    margin-top: 6rem;
    padding: 5rem 0;
}

.map_box {
    width: 67rem;
    height: 32rem;
    margin: 0 auto;
}

.map_left {
    height: 31rem;
    width: 60%;
    float: left;
}
.map_right{
    float: left;
    margin: 0.6rem 0 0 2rem;
    width: 36.7%;
}
.map_right p {
    color: #333333;
    font-family: 微软雅黑;

}
.map_right p i{
    display: block;
    float: left;
    width: 3rem;
    height: 3rem;
    background: url(/images/map_icon.png) no-repeat 0 -0.9rem;

}
.map_right p span{
    font-size: 1.2rem;
    display: block;
    padding-top: 0.7rem;
    font-family: 微软雅黑;
}
.map_right p span small{
    font-size: 1.1rem;
}
.map_right p b{
    display: block;
    font-weight: normal;
    font-size: 1.1rem;
    margin: -0.8rem 0px 1rem 6rem;
    font-family: 微软雅黑;
}
.map_right :nth-child(2) i{
    background: url(/images/map_icon.png) no-repeat 0 -114px;
}
.map_right :nth-child(3) i{
    background: url(/images/map_icon.png) no-repeat 0 -232px;
}


.course_box{
    width: 67rem;
    margin: 0 auto;
    text-align: center;
    padding: 5rem 0 5rem 0;
}
.course_box h4{
    font-size: 2rem;
    color: #333333;
    font-weight: normal;
    font-family: 微软雅黑;
}
.course_box p{
    font-size: 1rem;
    color: #666666;
    padding: 1rem 0 2rem 0;
    font-family: 微软雅黑;
}
.course_list{
    width: 100%;
    text-align: center;
}
.course_list th{
    font-weight: normal;
    color: white;
    background-color: #0e8f9e;
    height: 56px;
    border-right: 1px solid;
    letter-spacing: 5px;
    font-family: 微软雅黑;
}
.course_list th:hover{
    background-color: #0ac599;
}

.course_list td{
    height: 100px;
    border-right:1px solid #e8eff4;
    font-size: 17px;
    color: #666666;
    font-family: 微软雅黑;
}
.course_list td:last-child{
    border-right:1px solid white;
    background: url(/images/int_down.png) no-repeat center;
}
.course_list td:last-child a{
    display: block;
    width: 100%;
    height: 100%;
}
.course_list tr:nth-child(2n+1){background:#e2e9ef;}
.course_list tr:nth-child(n+1):hover{
    background-color: #8CD2E5;
}

.course_list tr:nth-child(2n){background:#f3f7fa;}

.course_list td:nth-child(5),.course_list td:nth-child(6){
    padding: 0 20px;
    text-align: left;
}

.tips{
    width: 100%;
    /* height: 350px; */
    background-color: #f4f7fa;
    padding: 3rem 0;
}
.tips_cont{
    width: 54%;
    height: 12rem;
  /*  border: 3px dashed #0e8f9e;*/
    margin: 0 auto;
   /* background: url(/images/yw_icon.png) no-repeat 122px 38px;*/
}

.tips_text{
    width: 51%;
    height: 12rem;
    /* margin: 34px auto; */
    text-align: center;
    float: left;
    border-left: 0.1rem dotted #666666;
}
.tips_text h4{
    font-weight: normal;
    font-size: 1rem;
    color: #333333;
    font-family: 微软雅黑;
    margin: 0.2rem 0 0.3rem 0;
}
.tips_text h4 i{
    display: inline-block;
    height: 3rem;
    width: 3rem;
    background: url(/images/ts_icon.png) no-repeat center;
}
.tips_text p{
    font-size: 0.97rem;
    color: #999999;
    line-height: 1.6rem;
    margin-top: 1.2rem;
    font-family: 微软雅黑;
    letter-spacing: 0.1rem;
}
.tips .consulting{
    width: 49%;
    height: 12rem;
    border-left: 0.1rem dotted #666666;
    border-right: 0.1rem dotted #666666;
    margin: 0 auto;
    /* padding: 12px 0; */
    text-align: center;
    float: right;
}
.tips .consulting span{
    display: block;
    margin-top: 0.4rem;
    margin-bottom: 0.9rem;
    font-size: 1.4rem;
    color: #333333;
    font-family: 微软雅黑;
    position: relative;
    letter-spacing: 0.5rem;
}
.tips .consulting span:before, .tips .consulting span:after{
    display: block;
    content: "";
    width: 3rem;
    border: 0.1rem solid #333333;
    margin-top: 1.1rem;

}
.tips .consulting span:before{
    margin-left: 5rem;
    float: left;
}
.tips .consulting span:after{
    margin-right:5rem;
    float: right;

}
.consulting img{
    width: 23%;
}
@media (max-width: 1600px){
    .top_banner .int_text span{
        left: 28%;
        font-size: 1.3rem;
    }
    .top_banner .int_text h2{
        font-size: 3.4rem;
    }
    .top_banner .int_text p {
        font-size: 1.2rem;
        padding-left: 3rem;
    }

}
@media (max-width: 1024px){
    .top_banner .int_text{
        width: 60%;
    }
    .top_banner .int_text span {
        left: 32%;
    }
}
@media (max-width: 1000px){
    .intSummary{
        width: 90%;
    }
    .intSummary_cont img {
        width: 45%;
    }
    .intSummary_text{
        width: 50%;
    }
    .intSummary_text h5{
        padding-bottom: 0;
    }
    .int_list{
        width: 90%;
    }
    .map_box{
        width: 92%;
    }
    .course_box{
        width: 92%;
    }
    .map_left {
        width: 50%;
    }
    .int_list li:nth-child(2) {
        margin: 5rem 0.3rem 0 0.3rem;
    }
}
@media (max-width: 768px){
    .tips .consulting{
        border-right: none;
    }
    .intSummary_cont img{
        width: 100%;
    }
    .intSummary_text {
        width: 100%;
    }
    .intSummary_text h5 {
        padding-bottom: 0;
        text-align: center;
    }
    .intSummary{
        height: 62rem;
    }
    .int_list{
        width: 80%;
    }
    .int_list li{
        float: none;
        width: 80%;
        margin: 3rem auto;
    }
    .int_list li:nth-child(2) {
        margin:0 auto;
    }
    .map{
        height:44rem;
    }
    .map_left{
        width: 100%;
        height: 20rem;
    }
    .map_right{
        width: 100%;
    }
    .map_right p{
        float: left;
        width: 30%;
    }
    .map_right p:nth-child(2){
        width: 40%;
    }
    .map_right p b{
        margin: 0;
    }
    .map_right p i {
        background-position: 0 0.1rem;
        background-size: 2rem;
    }
    .map_right :nth-child(2) i{
        background-position: 0 4rem;
        background-size: 2rem;
    }
    .map_right :nth-child(3) i{
        background-position: 0 -8.5rem;
        background-size: 2rem;
    }
    .tips_cont{
        width: 100%;
    }
    .tips_text{
        border: none;
    }
    .small-menu{
       /* right: 21rem;*/
    }
    .course_list td:last-child{
        background-size: 2.5rem;
    }

}
@media (max-width: 414px){
    .tips_text p{
        letter-spacing: 0;
    }
    .tips_text{
        width: 59%;
    }
    .tips .consulting{

    }
    .tips .consulting{
        border: none;
    }
    .top_banner{
        height: 19rem;
    }
    .top_banner .int_text h2{
        padding-top: 0;
    }
    .course_list td:last-child {
        background-size: 1.8rem;
    }
    .top_banner .int_text {
        width: 93%;
        margin-left: 2rem;
    }
    .top_banner .int_text span {
        left: 25%;
    }
    .top_banner .int_text h2 {
        font-size: 3rem;
    }
    .intSummary {
        width: 94%;
    }
    .int_list {
        width: 100%;
    }
    .int_list li{
        height: 47rem;
    }
    .map_right{
        margin: 0;
    }
    .map {
        height: 48rem;
    }
    .int_list li{
        width: 94%;
    }
    .course_list td{
        font-size: 0.6rem;
        border: none;
    }
    .int_list li h6{
        font-size: 1.6rem;
    }
    .course_box{
        width: 98%;
    }
    .course_box{
        font-size: 0.8rem;
    }
    .course_list th{
        font-size: 0.8rem;
    }
    .course_list th{
        letter-spacing: 0;
    }
    #ath {
        width: 5%;
    }
    #bth {
        width: 0%;
    }
    #cth {
        width: 7%;
    }
    #dth {
        width: 0%;
    }
    #eth {
        width: 50%;
    }
    #fth {
        width: 0%;
    }
    #gth {
        width: 14%;
    }
    #hth {
        width: 50%;
    }
    .course_list td:nth-child(5), .course_list td:nth-child(6){
        padding: 0 0.1rem;
    }
    .top_banner .int_text p {
        font-size: 1rem;
         padding-left: 0rem;
    }
    .top_banner .int_text h2 {
        font-size: 2.6rem;
    }
    .int_text div{
        border: none;
    }
    .top_banner .int_text span{
        position: static;
        border: none;
        background: none;
    }

}
@media (max-width: 320px){
    .course_list td {
        font-size: 0.23rem;
        border: none;
        overflow: hidden;
        word-break: break-all;

    }
    td{
        font-size: 0.2rem;
    }
    .map_right p i{
        width: 2rem;
    }
    .top_banner .int_text span{
        margin-top: 1rem;
    }
}