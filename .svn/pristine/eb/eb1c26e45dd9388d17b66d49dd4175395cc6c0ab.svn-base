/* tab */
.interview-tab-big {
    background-color: #F9F9F9;
    margin: 20px 15px;
    height: 712px;
}

.interview-tab-big h2 {
    padding-left: 20px;
}

.interview-tab {
    position: relative;
    margin: 20px auto 40px;
}

.interview-tab .interview-tab-menu li, .interview-tab .interview-tab-box {
    border: 1px solid #ccc;
}

.interview-tab .interview-tab-menu {
    width: 100%;
    float: left;
    position: absolute;
    z-index: 1;
}

.interview-tab .interview-tab-menu .selected {
    background-color: #000;
}

/*Tab切换*/
.interview-tab .interview-tab-menu li {
    font-size: 16px;
    color: #000;
    float: left;
    width: 50%;
    height: 50px;
    line-height: 50px;
    border-bottom-color: transparent;
    cursor: pointer;
    text-align: center;
}

.interview-tab .interview-tab-menu li:last-child {
    border-left-color: transparent;
}

.interview-tab .interview-tab-box {
    top: 50px;
    position: relative;
    border-top-color: transparent;
}

.interview-tab .interview-tab-menu .selected {
    cursor: pointer;
    color: #fff;
    border: 1px solid #000;
}

.hide {
    display: none;
}

.interview-tab-box div {
    position: relative;
    height: 590px;
    overflow: hidden;
    width: 100%;
}

.interview-tab-box div .tab-box-img {
    max-height: 100%;
    max-width: 100%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    position: absolute;
    left: 50%;
    top: 50%;
}

@media (max-width: 768px) {
    .user-edit-interview-modelOne a {
        font-size: 12px;
        padding: 10px 14px;
    }

    .interview-tab-big {
        height: 525px;
    }

    .interview-tab-box div {
        height: 400px;
    }
}

.interview-tab-box i {
    font-size: 70px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    color: #1BB38C;
}

