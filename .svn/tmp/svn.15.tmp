.mid_cont {
    width: 65rem;
    margin: 0 auto;
}

.enroll_box {
    width: 65rem;
    height: 55rem;
    background-color: #f3f6f9;
    margin-top: 2.3rem;
    padding: 3.75rem 2.75rem;
    position: relative;
}

@media (max-width: 750px) {
    .mid_cont {
        width: 65rem;
        margin: 0 auto;
    }

    .enroll_box {
        height: 27.5rem;
        background-color: #f3f6f9;
        margin-top: 1rem;
        padding: 3.75rem 2.75rem;
        position: relative;
    }

    .enroll_box .enroll_btn {
        height: 8.75rem;
        width: 8.75rem;
        background: url(/images/CIS_04.png) no-repeat center;
        position: absolute;
        top: 50%;
        left: 50%;
        margin-left: -8.75rem;
        margin-top: -8.75rem;
        text-align: center;
    }
}

.enroll_box div:nth-child(1) {
    padding-left: 1.75rem;
    padding-bottom: 7.9rem;
}

.enroll_box div {
    position: relative;
}

.enroll_top p {
    width: 25.4rem;
    position: absolute;
    top: 2.15rem;
    right: 2.8rem;
    font-size: 0.9rem;
    font-family: "微软雅黑";
    color: #333333;
    line-height: 1.65rem;
}

.enroll_top p span {
    font-size: 1.4rem;
    font-family: "微软雅黑";
}

.enroll_bottom p {
    width: 28rem;
    position: absolute;
    left: 2rem;
    top: 1.8rem;
    font-size: 1.4rem;
    font-family: "微软雅黑";
}

.enroll_bottom p span {
    display: block;
    font-size: 0.9rem;
    background: url(/images/CIS_06.png) no-repeat 1px 9px;
    height: 3.25rem;
    padding-left: 2.7rem;
    font-family: "微软雅黑";
    color: #333333;
}

.enroll_box .enroll_btn {
    height: 17.5rem;
    width: 17.5rem;
    background: url(/images/CIS_04.png) no-repeat center;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -8.75rem;
    margin-top: -8.75rem;
    text-align: center;
}

.enroll_btn a {
    display: block;
    width: 8.2rem;
    height: 8.2rem;
    background-color: #1e919e;
    border-radius: 100%;
    text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -4.1rem;
    margin-top: -4.1rem;
    line-height: 8.2rem;
    text-decoration: none;
    font-size: 1.45rem;
    color: #FFFFFF;
    font-family: "微软雅黑";
    font-weight: bold;
    letter-spacing: 0.1rem;
}

.enroll_btn a:hover {
    background-color: #2dafbe;
}

@media (max-width: 750px) {
    .enroll_box {
        width: 32.5rem;
        height: 30rem;
        background-color: #f3f6f9;
        margin-top: 2rem;
        padding: 3.75rem 1.25rem;
        position: relative;
        overflow: hidden;
    }

    .enroll_box div:nth-child(1) {
        height: 9.2rem;
        padding-left: 0;
    }

    .enroll_box div:nth-child(2) {
        height: 12rem;
        padding-left: 0;
        margin-top: 5rem;
    }

    .enroll_top img {
        width: 95%;
        float: right;
    }

    .enroll_top p {
        width: 13rem;
        position: absolute;
        top: 0.5rem;
        right: 0.2rem;
        line-height: 1.5rem;
        text-align: left;
    }

    .enroll_top p span {
        font-size: 1.2rem;
    }

    .enroll_bottom img {
        width: 95%;
    }

    .enroll_bottom p {
        width: 18rem;
        position: absolute;
        left: 0rem;
        top: 0.2rem;
        font-size: 1.2rem;
    }

    .enroll_bottom p span {
        display: block;
        background: url(/images/CIS_06.png) no-repeat 0.05rem 0.3rem;
        background-size: 5%;
        width: 16rem;
        height: auto;
        padding-left: 1.6rem;
        font-size: 0.8rem;
    }

    .enroll_top p i {
        display: inline-block;
        margin-top: -1rem;
    }

    .enroll_bottom p span:nth-of-type(1) {
        margin-top: -1rem;
    }

    .enroll_box .enroll_btn {
        height: 10rem;
        width: 10rem;
        background: url(/images/CIS_04.png) no-repeat center;
        background-size: 100%;
        position: absolute;
        top: 50%;
        left: 50%;
        margin-left: -5rem;
        margin-top: -5rem;
        text-align: center;
    }

    .enroll_btn a {
        display: block;
        width: 5rem;
        height: 5rem;
        background: #1e919e;
        border-radius: 100%;
        position: absolute;
        top: 50%;
        left: 50%;
        margin-left: -2.5rem;
        margin-top: -2.5rem;
        line-height: 5rem;
        text-decoration: none;
        font-size: 0.6rem;
    }

    .enroll_btn a:hover {
        background-color: #2dafbe;
    }
}

.application {
    height: 22rem;
    margin: 0 auto;
    text-align: center;
}

.application p {
    margin: 4.9rem 0 1.5rem 0;
}

@media (max-width: 750px) {
    .application {
        height: 22rem;
        position: relative;
        left: 0;
        top: 0;
        margin: 2rem 0;
        text-align: center;
    }

    .application p {
        margin: 4.9rem 0 1.5rem 0;
    }

    .consulting {
        width: 6rem;
        height: 12rem;
        border-left: 1px dotted #666666;
        border-right: 1px dotted #666666;
        margin: 0 auto;
        padding: 0.5rem 0;
    }

    .consulting img {
        width: 30%;
        height: auto;
    }

}

.project_details .btn {
    display: block;
    width: 10.3rem;
    height: 2.7rem;
    background-color: #29acbb;
    text-decoration: none;
    text-align: center;
    line-height: 2.7rem;
    font-size: 1.4rem;
    color: #FFFFFF;
    font-weight: bold;
    margin: 2.5rem auto;
    border-radius: 3.2rem;
    letter-spacing: 0.1rem;
}

.project_details .btn:hover {
    background-color: #32c3d3;
}

.application p a {
    font-size: 1.5rem;
    color: #333333;
    letter-spacing: 0.2rem;
}

@media (max-width: 750px) {
    .application p a {
        font-size: 1.2rem;
    }
}

.application .consulting {
    width: 22.25rem;
    height: 11.75rem;
    border-left: 1px dotted #666666;
    border-right: 1px dotted #666666;
    margin: 0 auto;
    padding: 0.6rem 0;
}

.application .consulting span {
    display: block;
    margin-top: 1.4rem;
    font-size: 1.25rem;
    color: #666;
    font-family: 微软雅黑;
    position: relative;
    letter-spacing: 0.45rem;
}

.application .consulting span:before, .application .consulting span:after {
    display: block;
    content: "";
    width: 2.5rem;
    border: 1px solid #999;
    margin-top: 1rem;

}

.application .consulting span:before {
    margin-left: 3.5rem;
    float: left;
}

.application .consulting span:after {
    margin-right: 3.5rem;
    float: right;

}

.consult_tel {
    display: block;
    width: 22rem;
    height: 2rem;
    background: url(/images/map_icon.png) no-repeat 2.6rem -13.3rem;
    position: relative;
    margin: 2rem auto;
    padding-left:3.6rem;
    line-height:2rem;
    font-size:1.2rem;
}

.project_details {
    width: 60rem;
    height: 55rem;
    background-color: #f3f6f9;
    border-right: 0.45rem solid #16a2b2;
    border-radius: 1rem;
    margin: 4rem auto 1.5rem auto;
}

.project_details h2 {
    padding: 4rem 0 2.5rem 5.5rem;
    color: #018c9c;
    font-size: 2rem;
}

.project_details .contens {
    width: 48.7rem;
    margin-left: 5.45rem;
    margin-bottom: 1rem;
    background: url(/images/CIS_05.png) no-repeat 0.1rem 0.05rem;
    padding-bottom: 0.5rem;
}

.project_details .contens p {
    padding-left: 4.2rem;
    padding-top: 1.15rem;
    font-family: 微软雅黑;
}

.project_details .contens b {
    font-size: 0.9rem;
    color: #333333;
    font-weight: normal;
    display: block;
    float: left;
    font-family: 微软雅黑;
}

.project_details .contens span {
    font-size: 0.9rem;
    color: #666666;
    display: inline-block;
    padding-left: 1.1rem;
    font-family: 微软雅黑;
    letter-spacing: 0.05rem;
}

.project_details .contens i {
    font-style: normal;
    font-size: 0.9rem;
    color: #999999;
}

@media (max-width: 750px) {
    .consult_tel {
        display: block;
        width: 22rem;
        height: 2rem;
        background: url(/images/map_icon.png) no-repeat 2.6rem -14.7rem;
