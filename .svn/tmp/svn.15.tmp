        border-radius: 1rem;
        margin: 0;
        /*overflow: hidden;*/
    }

    .project_details .contens {
        /*width: 48.7rem;*/
        width: 90%;
        /*height: 38.4rem;*/
        height: auto;
        margin-left: 2rem;
        background: url(/images/CIS_05.png) no-repeat 0rem 0.05rem;
        /*background: #f89;*/
    }

    .project_details h2 {
        padding: 4rem 0 2.5rem 2rem;
        color: #018c9c;
