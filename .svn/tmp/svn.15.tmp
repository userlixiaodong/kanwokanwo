.login-before {
    width: 35rem;
    height: 17rem;
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 1000;
    text-align: center;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    background: #e1e4e8;
    display: none;
}

.login-before .login-before-close-btn {
    color: #aaaaaa;
    position: absolute;
    right: 0.5rem;
    top: 0.5rem;
}

.login-before p {
    font-family: "Microsoft Yahei";
    font-size: 0.9rem;
    margin-top: 4.5rem;
    width: 100%;
    text-align: center;
}

.login-before .login-btn {
    display: block;
    width: 7rem;
    height: 2.75rem;
    font-size: 1.2rem;
    line-height: 2.75rem;
    color: #fff;
    text-align: center;
    margin: 3.5rem auto;
    background-color: #28b38d;
}
.leadFrame{
    display: none;
}

